') ('hoe', 'maatschappelijk') ('maatschappelijk', 'betrokken') ('betrokken', 'ik') ('ik', 'ben') ('ben', '.') ('.', 'Dit') ('Dit', 'komt') ('komt', 'voornamelijk') ('voornamelijk', 'door') ('door', 'het') ('het', 'feit') ('feit', 'dat') ('dat', 'ik') ('ik', 'nu') ('nu', 'hard') ('hard', 'aan') ('aan', 'het') ('het', 'werk') ('werk', 'ben') ('ben', 'om') ('om', 'af') ('af', 'te') ('te', 'studeren') ('studeren', '.') ('.', 'Hiervoor') ('Hiervoor', 'heb') ('heb', 'ik') ('ik', 'wel') ('wel', 'jeugdclub') ('jeugdclub', 'geleid') ('geleid', 'en') ('en', 'ben') ('ben', 'ik') ('ik', 'betrokken') ('betrokken', 'geweest') ('geweest', 'bij') ('bij', 'sportverenigingen') ('sportverenigingen', 'etc') ('etc', 'om') ('om', 'jongeren') ('jongeren', 'een') ('een', 'go') ('go', 'toekomstbeeld') ('toekomstbeeld', 'te') ('te', 'kunnen') ('kunnen', 'bieden') ('bieden', '.')