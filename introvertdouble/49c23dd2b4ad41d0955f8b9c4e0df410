eeship', 'verschillend') ('verschillend', 'functi') ('functi', 'en') ('en', 'de') ('de', 'werkwijz') ('werkwijz', 'van') ('van', 'de') ('de', 'overheid') ('overheid', 'te') ('te', 'kunnen') ('kunnen', 'ontdekken') ('ontdekken', '.') ('.', 'Een') ('Een', 'basi') ('basi', 'van') ('van', 'opleidingen') ('opleidingen', 'en') ('en', 'trainingen') ('trainingen', 'geeft') ('geeft', 'me') ('me', 'hierbij') ('hierbij', 'de') ('de', 'kan') ('kan', 'om') ('om', 'mijn') ('mijn', 'kwaliteiten') ('kwaliteiten', 'en') ('en', 'talenten') ('talenten', 'verder') ('verder', 'te') ('te', 'ontwikkelen') ('ontwikkelen', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'studi') ('studi', 'heb') ('heb', 'ik') ('ik', 'altijd') ('altijd', 'met') ('met', 'veel') ('veel', 'plezier') ('plezier', 'me') ('me', 'ingezet') ('ingezet', 'voor') ('voor', 'verschillend') ('verschillend', 'verenigingen') ('verenigingen', '.') ('.', 'Doorslaggevend') ('Doorslaggevend', 'wa') ('wa', 'voor') ('voor', 'mij') ('mij', 'echter') ('echter', 'een') ('een', 'vier') ('vier', 'maand') ('maand', 'durend') ('durend', 'rei') ('rei', 'door') ('door', 'Azi') ('Azi', 'na') ('na', 'mijn') ('mijn', 'studi') ('studi', ',') (',', 'waarbij') ('waarbij', 'ik') ('ik', 'ontwikkelingslanden') ('ontwikkelingslanden', 'al') ('al', 'Lao') ('Lao', 'en') ('en', 'Cambodja') ('Cambodja', 'heb') ('heb', 'aangedaan') ('aangedaan', ',') (',', 'werd') ('werd', 'ik') ('ik', 'met') ('met', 'mijn') ('mijn', 'neu') ('neu', 'op') ('op', 'de') ('de', 'feiten') ('feiten', 'gedrukt') ('gedrukt', '.') ('.', 'Kenni') ('Kenni', 'van') ('van', 'goed') ('goed', 'hygien') ('hygien', ',') (',', 'een') ('een', 'dak') ('dak', 'boven') ('boven', 'je') ('je', 'hoofd') ('hoofd', 'en') ('en', 'de') ('de', 'mogelijkheid') ('mogelijkheid', 'om') ('om', 'onderwij') ('onderwij', 'te') ('te', 'kunnen') ('kunnen', 'volgen') ('volgen', ',') (',', 'zijn') ('zijn', 'aspecten') ('aspecten', 'die') ('die', 'niet') ('niet', 'alleen') ('alleen', 'niet') ('niet', 'altijd') (