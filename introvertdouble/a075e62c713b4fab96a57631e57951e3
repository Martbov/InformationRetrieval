') ('uit', 'komen') ('komen', 'in') ('in', 'de') ('de', 'rol') ('rol', 'van') ('van', 'Rijkstraine') ('Rijkstraine', '.') ('.', 'Kritiek') ('Kritiek', 'hebben') ('hebben', 'op') ('op', 'plannen') ('plannen', ',') (',', 'wetten') ('wetten', 'en') ('en', 'beleid') ('beleid', 'van') ('van', 'de') ('de', 'overheid') ('overheid', 'is') ('is', 'altijd') ('altijd', 'makkelijk') ('makkelijk', ',') (',', 'ook') ('ook', 'ik') ('ik', 'heb') ('heb', 'dit') ('dit', '.') ('.', 'Al') ('Al', 'ga') ('ga', 'ik') ('ik', 'altijd') ('altijd', 'bij') ('bij', 'mezelf') ('mezelf', 'na') ('na', ':') (':', 'hoe') ('hoe', 'zou') ('zou', 'dit') ('dit', 'beter') ('beter', 'kunnen') ('kunnen', '?') ('?', 'Dan') ('Dan', 'pa') ('pa', 'zie') ('zie', 'je') ('je', 'hoe') ('hoe', 'lastig') ('lastig', 'sommig') ('sommig', 'dingen') ('dingen', 'kunnen') ('kunnen', 'zijn') ('zijn', ',') (',', 'je') ('je', 'kunt') ('kunt', 'het') ('het', 'haast') ('haast', 'niet') ('niet', 'go') ('go', 'doen') ('doen', 'voor') ('voor', 'iedereen') ('iedereen', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'politieke/maatschappelijk') ('politieke/maatschappelijk', 'discussi') ('discussi', 'altijd') ('altijd', 'erg') ('erg', 'interess') ('interess', 'omdat') ('omdat', 'er') ('er', 'veel') ('veel', 'te') ('te', 'zeggen') ('zeggen', 'valt') ('valt', 'voor') ('voor', 'all') ('all', 'partijen') ('partijen', 'die') ('die', 'betrokken') ('betrokken', 'zijn') ('zijn', '.') ('.', 'Het') ('Het', 'horen') ('horen', 'van') ('van', 'verschillend') ('verschillend', 'standpunten') ('standpunten', 'kan') ('kan', 'de') ('de', 'zaken') ('zaken', 'complex') ('complex', 'maken') ('maken', 'maar') ('maar', 'al') ('al', 'je') ('je', 'inziet') ('inziet', 'wat') ('wat', 'er') ('er', 'achter') ('achter', 'deze') ('deze', 'standpunten') ('standpunten', 'schuilt') ('schuilt', ',') (',', 'kun') ('kun', 'je') ('je', 'volgen') ('volgen', 'mij') ('mij', 'ook') ('ook', 'echt') ('echt', 'iet') ('iet', 'betekenen') ('betekenen', '.') ('.', 'Door') ('Door', 'mij