') ('go', 'eind') ('eind', 'te') ('te', 'brengen') ('brengen', '.') ('.', 'In') ('In', 'mijn') ('mijn', 'huidig') ('huidig', 'functi') ('functi', 'al') ('al', 'manag') ('manag', 'assist') ('assist', 'heb') ('heb', 'ik') ('ik', 'bewezen') ('bewezen', 'om') ('om', 'in') ('in', 'kort') ('kort', 'tijd') ('tijd', 'veel') ('veel', 'projecten') ('projecten', 'aan') ('aan', 'te') ('te', 'pakken') ('pakken', 'en') ('en', 'zelfstandig') ('zelfstandig', 'uit') ('uit', 'te') ('te', 'voeren') ('voeren', '.') ('.', 'Communiceren') ('Communiceren', 'met') ('met', 'veel') ('veel', 'verschillend') ('verschillend', 'partijen') ('partijen', 'is') ('is', 'een') ('een', 'essentieel') ('essentieel', 'onderdeel') ('onderdeel', 'van') ('van', 'mijn') ('mijn', 'baan') ('baan', ',') (',', 'en') ('en', 'mijn') ('mijn', 'collega¿') ('collega¿', 'waarderen') ('waarderen', 'mijn') ('mijn', 'accuratess') ('accuratess', 'en') ('en', 'dienstverlenend') ('dienstverlenend', 'instel') ('instel', '.') ('.', 'Met') ('Met', 'mijn') ('mijn', 'baan') ('baan', 'bij') ('bij', 'de') ('de', 'wetenschapswinkel') ('wetenschapswinkel', '(') ('(', 'Universiteit') ('Universiteit', 'Twent') ('Twent', ')') (')', 'ben') ('ben', 'ik') ('ik', 'in') ('in', 'contact') ('contact', 'gekomen') ('gekomen', 'met') ('met', 'veel') ('veel', 'verschillend') ('verschillend', 'maatschappelijk') ('maatschappelijk', 'organisati') ('organisati', 'en') ('en', 'belangengroepen') ('belangengroepen', 'die') ('die', 'behoeft') ('behoeft', 'hebben') ('hebben', 'aan') ('aan', 'wetenschappelijk') ('wetenschappelijk', 'onderzoek') ('onderzoek', 'op') ('op', 'zeer') ('zeer', 'uiteenlopend') ('uiteenlopend', 'terreinen') ('terreinen', '.') ('.', 'Ik') ('Ik', 'vond') ('vond', 'het') ('het', 'een') ('een', 'uitdag') ('uitdag', 'om') ('om', 'samen') ('samen', 'met') ('met', 'deze') ('deze', 'organisati') ('organisati', 'concret') ('concret', 'vraag-') ('vraag-', 'en') ('en', 'probleemstellingen') ('probleemstellingen', 'te') ('te', 'vormen') ('vorm