ikkelen', 'in') ('in', 'dit') ('dit', 'enorm') ('enorm', 'gevarieerd') ('gevarieerd', 'werkgebi') ('werkgebi', 'zal') ('zal', 'ik') ('ik', 'met') ('met', 'veel') ('veel', 'plezier') ('plezier', 'en') ('en', 'inzet') ('inzet', 'benutten') ('benutten', '.') ('.', 'Ik') ('Ik', 'geloof') ('geloof', 'er') ('er', 'sterk') ('sterk', 'in') ('in', 'dat') ('dat', 'verand') ('verand', 'pa') ('pa', 'mogelijk') ('mogelijk', 'is') ('is', 'al') ('al', 'elk') ('elk', 'burger') ('burger', 'zich') ('zich', 'betrokken') ('betrokken', '?') ('?', 'en') ('en', 'daarme') ('daarme', 'verantwoordelijk') ('verantwoordelijk', '?') ('?', 'voelt') ('voelt', 'voor') ('voor', 'zijn') ('zijn', 'of') ('of', 'haar') ('haar', 'leefomgev') ('leefomgev', '.') ('.', 'Ik') ('Ik', 'geef') ('geef', 'daar') ('daar', 'zelf') ('zelf', 'uit') ('uit', 'aan') ('aan', 'door') ('door', ':') (':', '*') ('*', 'zit') ('zit', 'te') ('te', 'hebben') ('hebben', 'genomen') ('genomen', 'in') ('in', 'advies-') ('advies-', 'en') ('en', 'bestuursfuncti') ('bestuursfuncti', 'en') ('en', 'zo') ('zo', 'op') ('op', 'te') ('te', 'treden') ('treden', 'al') ('al', 'spreekbui') ('spreekbui', '*') ('*', 'al') ('al', 'coordin') ('coordin', 'internationalis') ('internationalis', 'het') ('het', 'blikveld') ('blikveld', 'van') ('van', 'studenten') ('studenten', 'verruimd') ('verruimd', 'te') ('te', 'hebben') ('hebben', 'door') ('door', 'het') ('het', 'bieden') ('bieden', 'van') ('van', 'een') ('een', 'international') ('international', 'ervar') ('ervar', '*') ('*', 'het') ('het', 'volgen') ('volgen', 'van') ('van', 'politiek') ('politiek', 'en') ('en', 'actualiteiten') ('actualiteiten', ',') (',', 'zodat') ('zodat', 'vooroordelen') ('vooroordelen', 'weerlegd') ('weerlegd', 'kunnen') ('kunnen', 'worden') ('worden', '*') ('*', '?') ('?', 'groener') ('groener', '?') ('?', 'te') ('te', 'leven') ('leven', 'door') ('door', 'afval') ('afval', 'te') ('te', 'scheiden') ('scheiden', 'en') ('en', 'met') ('met', 'OV') ('OV', 'te') ('te', 'reizen') ('