ed') ('breed', 'te') ('te', 'ontwikkelen') ('ontwikkelen', '.') ('.', 'Het') ('Het', 'is') ('is', 'een') ('een', 'perfect') ('perfect', 'kan') ('kan', 'de') ('de', 'overheid') ('overheid', 'te') ('te', 'leren') ('leren', 'kennen') ('kennen', 'en') ('en', 'te') ('te', 'ontdekken') ('ontdekken', 'waar') ('waar', 'exact') ('exact', 'mijn') ('mijn', 'interess') ('interess', 'en') ('en', 'kwaliteiten') ('kwaliteiten', 'liggen') ('liggen', '.') ('.', 'Ondank') ('Ondank', 'mijn') ('mijn', 'keuz') ('keuz', 'voor') ('voor', 'een') ('een', 'bedrijfskundig') ('bedrijfskundig', 'studi') ('studi', ',') (',', 'heb') ('heb', 'ik') ('ik', 'mijn') ('mijn', 'projecten') ('projecten', 'altijd') ('altijd', 'bewust') ('bewust', 'uitgekozen') ('uitgekozen', 'op') ('op', 'zeker') ('zeker', 'maatschappelijk') ('maatschappelijk', 'waard') ('waard', ',') (',', 'zodat') ('zodat', 'het') ('het', 'beoogd') ('beoogd', 'doel') ('doel', 'voor') ('voor', 'mij') ('mij', 'waardevol') ('waardevol', 'wa') ('wa', '.') ('.', 'Voor') ('Voor', 'mijn') ('mijn', 'bachelorproject') ('bachelorproject', 'heb') ('heb', 'ik') ('ik', 'gekeken') ('gekeken', 'naar') ('naar', 'de') ('de', 'mogelijkheid') ('mogelijkheid', 'voor') ('voor', 'MKB') ('MKB', 'bouwondernemingen') ('bouwondernemingen', 'om') ('om', 'een') ('een', 'Publiek') ('Publiek', 'Privaat') ('Privaat', 'Samenwerkingscontract') ('Samenwerkingscontract', 'aan') ('aan', 'te') ('te', 'gaan') ('gaan', 'met') ('met', 'de') ('de', 'overheid') ('overheid', '.') ('.', 'Bij') ('Bij', 'een') ('een', 'thuiszorgorganisati') ('thuiszorgorganisati', 'heb') ('heb', 'ik') ('ik', 'onderzocht') ('onderzocht', 'of') ('of', 'door') ('door', 'samenwerk') ('samenwerk', 'de') ('de', 'zorg') ('zorg', 'efficient') ('efficient', 'en') ('en', 'beter') ('beter', 'georganiseerd') ('georganiseerd', 'kon') ('kon', 'worden') ('worden', '.') ('.', 'Het') ('Het', 'doel') ('doel', 'van') ('van', 'mijn') ('mijn', 'afstudeeronderzoek') ('afstudeeronderzoek', 'wa') ('wa', 'om') ('om', 'de')