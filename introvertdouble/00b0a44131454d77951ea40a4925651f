') ('de', 'praktijk') ('praktijk', 'toe') ('toe', 'te') ('te', 'kunnen') ('kunnen', 'passen') ('passen', '.') ('.', 'Vanweg') ('Vanweg', 'mijn') ('mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'zie') ('zie', 'ik') ('ik', 'het') ('het', 'Rijk') ('Rijk', 'al') ('al', 'een') ('een', 'ideal') ('ideal', 'werkgev') ('werkgev', ',') (',', 'waar') ('waar', 'ik') ('ik', 'mijn') ('mijn', 'brede') ('brede', 'vakinhoudelijk') ('vakinhoudelijk', 'kenni') ('kenni', 'alsmed') ('alsmed', 'mijn') ('mijn', 'maatschappelijk') ('maatschappelijk', 'kenni') ('kenni', 'kan') ('kan', 'inzetten') ('inzetten', 'ten') ('ten', 'gunst') ('gunst', 'voor') ('voor', 'de') ('de', 'ontwikkel') ('ontwikkel', 'van') ('van', 'de') ('de', 'samenlev') ('samenlev', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'op') ('op', 'verschillend') ('verschillend', 'manieren') ('manieren', 'maatschappelijk') ('maatschappelijk', 'betrokken') ('betrokken', '.') ('.', 'Deze') ('Deze', 'activiteiten') ('activiteiten', 'vinden') ('vinden', 'voornamelijk') ('voornamelijk', 'plaat') ('plaat', 'in') ('in', 'mijn') ('mijn', 'geboorte-') ('geboorte-', 'en') ('en', 'woonplaat') ('woonplaat', 'Oud-Beijerland') ('Oud-Beijerland', ',') (',', 'Zuid-Holland') ('Zuid-Holland', '.') ('.', 'De') ('De', 'meest') ('meest', 'aansprekend') ('aansprekend', 'voorbeelden') ('voorbeelden', 'zijn') ('zijn', 'mijn') ('mijn', 'betrokkenheid') ('betrokkenheid', 'bij') ('bij', 'de') ('de', 'winkeliersverenig') ('winkeliersverenig', 'en') ('en', 'Oranjeverenig') ('Oranjeverenig', '.') ('.', 'Al') ('Al', 'actief') ('actief', 'lid') ('lid', 'van') ('van', 'deze') ('deze', 'twee') ('twee', 'verenigingen') ('verenigingen', 'ben') ('ben', 'ik') ('ik', 'o.a') ('o.a', '.') ('.', 'betrokken') ('betrokken', 'bij') ('bij', 'de') ('de', 'organisati') ('organisati', 'van') ('van', 'de') ('de', 'Intocht') ('Intocht', 'van') ('van', 'Sinterklaa') ('Sinterklaa', ',') (',', 'Koninginnedag') ('Koninginnedag', ',') (',', 'Bevrijdingsdag'