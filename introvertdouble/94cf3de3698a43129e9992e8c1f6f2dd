', 'van') ('van', 'mijn') ('mijn', 'vakgebi') ('vakgebi', ',') (',', 'bijvoorbeeld') ('bijvoorbeeld', 'een') ('een', 'referaat') ('referaat', 'over') ('over', 'concentratiewijken') ('concentratiewijken', '.') ('.', 'Verder') ('Verder', 'kijk') ('kijk', 'ik') ('ik', 'geregeld') ('geregeld', 'naar') ('naar', 'het') ('het', 'nieuw') ('nieuw', 'en') ('en', 'divers') ('divers', 'actualiteitenprogramma') ('actualiteitenprogramma', "'s") ("'s", 'Interessant') ('Interessant', 'thema') ('thema', "'s") ("'s", 'vind') ('vind', 'ik') ('ik', ':') (':', 'ruimtelijk') ('ruimtelijk', 'orden') ('orden', ',') (',', 'wonen') ('wonen', ',') (',', 'integrati') ('integrati', ',') (',', 'veiligheid') ('veiligheid', ',') (',', 'milieu') ('milieu', ',') (',', 'cultuur') ('cultuur', ',') (',', 'welzijn') ('welzijn', ',') (',', 'recreati') ('recreati', ',') (',', 'onderwij') ('onderwij', 'en') ('en', 'infrastructuur')