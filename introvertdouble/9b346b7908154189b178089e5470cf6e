et', 'volgen') ('volgen', 'van') ('van', 'het') ('het', 'nieuw') ('nieuw', 'en') ('en', 'het') ('het', 'lezen') ('lezen', 'van') ('van', 'opiniestukken') ('opiniestukken', ',') (',', 'heeft') ('heeft', 'dit') ('dit', 'zich') ('zich', 'vooral') ('vooral', 'geuit') ('geuit', 'in') ('in', 'mijn') ('mijn', 'werk') ('werk', 'al') ('al', 'vrijwillig') ('vrijwillig', 'voor') ('voor', 'Vluchtelingenwerk') ('Vluchtelingenwerk', ',') (',', 'waarbij') ('waarbij', 'ik') ('ik', 'al') ('al', 'juridisch') ('juridisch', 'adviseur') ('adviseur', 'op') ('op', 'het') ('het', 'Asielzoekerscentrum') ('Asielzoekerscentrum', 'te') ('te', 'Nijmegen') ('Nijmegen', 'een') ('een', 'jaar') ('jaar', 'lang') ('lang', 'de') ('de', 'asielprocedur') ('asielprocedur', 'van') ('van', 'vluchtelingen') ('vluchtelingen', 'heb') ('heb', 'begeleid') ('begeleid', '.') ('.', 'Ook') ('Ook', 'al') ('al', 'bestuurslid') ('bestuurslid', 'en') ('en', 'inmiddel') ('inmiddel', 'lid') ('lid', 'van') ('van', 'de') ('de', 'Raad') ('Raad', 'van') ('van', 'Advi') ('Advi', 'van') ('van', 'studentenorganisati') ('studentenorganisati', 'Unit') ('Unit', 'Netherland') ('Netherland', 'is') ('is', 'dit') ('dit', 'tot') ('tot', 'uit') ('uit', 'gekomen') ('gekomen', '.') ('.', 'De') ('De', 'studentenorganisati') ('studentenorganisati', 'organiseert') ('organiseert', 'activiteiten') ('activiteiten', 'waarbij') ('waarbij', 'kwesti') ('kwesti', 'aan') ('aan', 'de') ('de', 'ord') ('ord', 'komen') ('komen', 'die') ('die', 'op') ('op', 'dat') ('dat', 'moment') ('moment', 'de') ('de', 'aandacht') ('aandacht', 'van') ('van', 'de') ('de', 'jeugd') ('jeugd', 'verdienen') ('verdienen', ',') (',', 'zoal') ('zoal', 'klimaatverand') ('klimaatverand', ',') (',', 'nucleair') ('nucleair', 'proliferati') ('proliferati', 'en') ('en', 'mensenrechten') ('mensenrechten', '.')