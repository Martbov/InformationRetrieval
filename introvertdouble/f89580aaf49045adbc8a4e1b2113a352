') ('enorm', 'uitdag') ('uitdag', '.') ('.', 'Wat') ('Wat', 'mij') ('mij', 'vooral') ('vooral', 'leuk') ('leuk', 'lijkt') ('lijkt', 'is') ('is', 'dat') ('dat', 'je') ('je', 'het') ('het', 'hele') ('hele', 'proce') ('proce', 'van') ('van', 'ontwikkel') ('ontwikkel', 'tot') ('tot', 'uitvoer') ('uitvoer', 'meemaakt') ('meemaakt', '.') ('.', 'Op') ('Op', 'deze') ('deze', 'manier') ('manier', 'kunnen') ('kunnen', 'je') ('je', 'achteraf') ('achteraf', 'voor') ('voor', 'jezelf') ('jezelf', 'go') ('go', 'analyseren') ('analyseren', 'welk') ('welk', 'dingen') ('dingen', 'er') ('er', 'in') ('in', 'het') ('het', 'proce') ('proce', 'go') ('go', 'zijn') ('zijn', 'gegaan') ('gegaan', 'en') ('en', 'welk') ('welk', 'dingen') ('dingen', 'minder') ('minder', 'go') ('go', '.') ('.', 'Daarnaast') ('Daarnaast', 'kun') ('kun', 'je') ('je', 'op') ('op', 'deze') ('deze', 'manier') ('manier', 'het') ('het', 'eigen') ('eigen', 'handelen') ('handelen', 'go') ('go', 'reflecteren') ('reflecteren', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'het') ('het', 'belangrijk') ('belangrijk', 'om') ('om', 'maatschappelijk') ('maatschappelijk', 'iet') ('iet', 'bij') ('bij', 'te') ('te', 'dragen') ('dragen', 'en') ('en', 'te') ('te', 'betekenen') ('betekenen', '.') ('.', 'Tijden') ('Tijden', 'het') ('het', 'proce') ('proce', 'van') ('van', 'het') ('het', 'ontwikkelen') ('ontwikkelen', 'van') ('van', 'een') ('een', 'beleid') ('beleid', 'vind') ('vind', 'ik') ('ik', 'het') ('het', 'belangrijk') ('belangrijk', 'om') ('om', 'elk') ('elk', 'betrokkenen') ('betrokkenen', 'het') ('het', 'gevoel') ('gevoel', 'te') ('te', 'geven') ('geven', 'dat') ('dat', 'zij') ('zij', 'gehoord') ('gehoord', 'worden') ('worden', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'het') ('het', 'daarom') ('daarom', 'belangrijk') ('belangrijk', 'dat') ('dat', 'een') ('een', 'deel') ('deel', 'van') ('van', 'hun') ('hun', 'ideeën') ('ideeën', 'en') ('en', 'voorstellen') ('voorstellen', 'ook') ('ook', 'in') ('in', 'het') ('het', 'definitiev') ('de