rlands') ('Nederlands', 'en') ('en', 'Europes') ('Europes', 'wet-') ('wet-', 'en') ('en', 'regelgeving.Ik') ('regelgeving.Ik', 'zie') ('zie', 'nu') ('nu', 'hoe') ('hoe', 'belangrijk') ('belangrijk', 'het') ('het', 'is') ('is', 'om') ('om', 'een') ('een', 'go') ('go', 'beleid') ('beleid', 'te') ('te', 'creëren') ('creëren', 'voor') ('voor', 'een') ('een', 'natuurgebi') ('natuurgebi', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'stage') ('stage', 'heb') ('heb', 'ik') ('ik', 'veel') ('veel', 'gewerkt') ('gewerkt', 'met') ('met', 'ArcGI') ('ArcGI', 'en') ('en', 'Definien') ('Definien', 'Develop') ('Develop', '(') ('(', 'luchtfoto-interpretati') ('luchtfoto-interpretati', ')') (')', '.') ('.', 'Deze') ('Deze', 'programma') ('programma', "'s") ("'s", 'zijn') ('zijn', 'van') ('van', 'groot') ('groot', 'belang') ('belang', 'voor') ('voor', 'het') ('het', 'beheren') ('beheren', 'van') ('van', 'natuurgebieden') ('natuurgebieden', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'ze') ('ze', 'toegepast') ('toegepast', 'voor') ('voor', 'natuurbehoud') ('natuurbehoud', 'in') ('in', 'de') ('de', 'Guiana') ('Guiana', "'s") ("'s", 'en') ('en', 'voor') ('voor', 'beheer') ('beheer', 'in') ('in', 'de') ('de', 'Nederlands') ('Nederlands', 'duinen') ('duinen', '.') ('.', 'Juist') ('Juist', 'in') ('in', 'een') ('een', 'dichtbevolkt') ('dichtbevolkt', 'gebi') ('gebi', 'al') ('al', 'Nederland') ('Nederland', 'is') ('is', 'het') ('het', 'essentieel') ('essentieel', 'dat') ('dat', 'er') ('er', 'op') ('op', 'zorgzam') ('zorgzam', 'wijz') ('wijz', 'wordt') ('wordt', 'omgegaan') ('omgegaan', 'met') ('met', 'de') ('de', 'schaars') ('schaars', 'natuur') ('natuur', '.')