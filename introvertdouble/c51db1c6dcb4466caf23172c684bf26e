te') ('te', 'hebben') ('hebben', 'voor') ('voor', 'economisch') ('economisch', 'aspecten') ('aspecten', 'maar') ('maar', 'ook') ('ook', 'voor') ('voor', 'de') ('de', 'men') ('men', 'en') ('en', 'de') ('de', 'maatschappij') ('maatschappij', '.') ('.', 'Daarnaast') ('Daarnaast', 'ben') ('ben', 'ik') ('ik', 'me') ('me', 'meer') ('meer', 'gaan') ('gaan', 'beseffen') ('beseffen', 'dat') ('dat', 'je') ('je', 'samen') ('samen', 'al') ('al', 'team') ('team', 'veel') ('veel', 'kunt') ('kunt', 'bereiken') ('bereiken', '.') ('.', '*') ('*', 'HAL') ('HAL', ',') (',', 'Coordin') ('Coordin', 'Huiswerkbeg') ('Huiswerkbeg', ':') (':', 'Het') ('Het', 'managen') ('managen', 'van') ('van', 'project') ('project', 'HAL') ('HAL', '.') ('.', 'Contacten') ('Contacten', 'onderhouden') ('onderhouden', 'met') ('met', 'gemeent') ('gemeent', ',') (',', 'scholen') ('scholen', 'en') ('en', 'ouder') ('ouder', '.') ('.', 'Coordineren') ('Coordineren', 'van') ('van', 'de') ('de', 'dagelijks') ('dagelijks', 'activiteiten') ('activiteiten', 'en') ('en', 'het') ('het', 'begeleiden') ('begeleiden', 'en') ('en', 'aansturen') ('aansturen', 'van') ('van', 'mentoren') ('mentoren', '.') ('.', '*') ('*', 'Social') ('Social', 'coach') ('coach', ':') (':', 'het') ('het', 'bieden') ('bieden', 'van') ('van', 'social') ('social', 'begeleid') ('begeleid', 'aan') ('aan', 'kinderen') ('kinderen', 'van') ('van', 'basisscholen') ('basisscholen', 'en') ('en', 'voortgezet') ('voortgezet', 'onderwij') ('onderwij', '.') ('.', '*') ('*', 'Huiswerkbegeleid') ('Huiswerkbegeleid', ':') (':', 'al') ('al', 'mentor') ('mentor', 'educatiev') ('educatiev', 'begeleid') ('begeleid', 'aan') ('aan', 'kinderen') ('kinderen', 'uit') ('uit', 'groep') ('groep', '8') ('8', 'en') ('en', 'onderbouw') ('onderbouw', 'voortgezet') ('voortgezet', 'onderwij') ('onderwij', '.') ('.', '*') ('*', 'Actief') ('Actief', 'met') ('met', 'politiek') ('politiek', ',') (',', 'jongerenwerk') ('jongerenwerk', ',') (',', 'stichtingen') ('stichtingen', 'en') ('e