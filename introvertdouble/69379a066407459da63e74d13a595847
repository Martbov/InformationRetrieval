') ('beleid', 'en') ('en', 'hoe') ('hoe', 'dit') ('dit', 'vertaald') ('vertaald', 'zou') ('zou', 'moeten') ('moeten', 'worden') ('worden', 'naar') ('naar', 'de') ('de', 'leden') ('leden', '.') ('.', 'Al') ('Al', 'deze') ('deze', 'ervaringen') ('ervaringen', 'maken') ('maken', 'voor') ('voor', 'mij') ('mij', 'duidelijk') ('duidelijk', 'dat') ('dat', 'ik') ('ik', 'graag') ('graag', 'wat') ('wat', 'voor') ('voor', 'anderen') ('anderen', 'beteken') ('beteken', 'en') ('en', 'dat') ('dat', 'ik') ('ik', 'in') ('in', 'staat') ('staat', 'ben') ('ben', 'om') ('om', 'de') ('de', 'kar') ('kar', 'te') ('te', 'trekken') ('trekken', 'van') ('van', 'lastig') ('lastig', 'projecten') ('projecten', '.') ('.', 'Deze') ('Deze', 'kwaliteiten') ('kwaliteiten', 'wil') ('wil', 'ik') ('ik', 'dan') ('dan', 'ook') ('ook', 'graag') ('graag', 'verder') ('verder', 'ontwikkelen') ('ontwikkelen', 'in') ('in', 'mijn') ('mijn', 'werkomgev') ('werkomgev', 'en') ('en', 'een') ('een', 'Rijkstraineeprogramma') ('Rijkstraineeprogramma', 'bie') ('bie', 'hier') ('hier', 'mooie') ('mooie', 'kansen') ('kansen', 'voor') ('voor', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'naast') ('naast', 'mijn') ('mijn', 'studi') ('studi', '(') ('(', 'en') ('en', 'naast') ('naast', 'de') ('de', 'zeilverenig') ('zeilverenig', ')') (')', 'altijd') ('altijd', 'gewerkt') ('gewerkt', '.') ('.', 'Deze') ('Deze', 'bijbanen') ('bijbanen', 'waren') ('waren', 'met') ('met', 'name') ('name', 'in') ('in', 'de') ('de', 'gezondheidszorg') ('gezondheidszorg', '.') ('.', 'Mijn') ('Mijn', 'werk') ('werk', 'al') ('al', 'thuishulp') ('thuishulp', 'A') ('A', 'bij') ('bij', 'de') ('de', 'thuiszorg') ('thuiszorg', 'en') ('en', 'de') ('de', 'laatst') ('laatst', 'jaren') ('jaren', 'al') ('al', 'afdelingsassist') ('afdelingsassist', 'in') ('in', 'het') ('het', 'Martini') ('Martini', 'Ziekenhui') ('Ziekenhui', 'in') ('in', 'Groningen') ('Groningen', 'heb') ('heb', 'ik') ('ik', 'al') ('al', 'erg') ('erg', 'prettig') ('prettig', 'ervaren') ('ervaren', '.')