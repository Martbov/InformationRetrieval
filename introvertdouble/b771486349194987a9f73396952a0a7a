', 'ben') ('ben', 'ik') ('ik', 'aan') ('aan', 'mijn') ('mijn', 'studi') ('studi', 'Nederland') ('Nederland', 'recht') ('recht', 'begonnen') ('begonnen', '.') ('.', 'Daarnaast') ('Daarnaast', 'blijkt') ('blijkt', 'mijn') ('mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'uit') ('uit', 'het') ('het', 'feit') ('feit', 'dat') ('dat', 'ik') ('ik', 'al') ('al', 'verschillend') ('verschillend', 'jaren') ('jaren', 'tijden') ('tijden', 'de') ('de', 'verkiezingen') ('verkiezingen', 'werkzaam') ('werkzaam', 'ben') ('ben', 'al') ('al', 'lid/') ('lid/', 'voorzitt') ('voorzitt', 'van') ('van', 'het') ('het', 'stembureau') ('stembureau', 'in') ('in', 'de') ('de', 'gemeent') ('gemeent', 'Vught') ('Vught', '.') ('.', 'Hier') ('Hier', 'zorg') ('zorg', 'ik') ('ik', 'met') ('met', 'de') ('de', 'ander') ('ander', 'leden') ('leden', 'van') ('van', 'het') ('het', 'stembureau') ('stembureau', 'voor') ('voor', 'een') ('een', 'ordelijk') ('ordelijk', 'verloop') ('verloop', 'van') ('van', 'de') ('de', 'verkiezingen') ('verkiezingen', 'en') ('en', 'volg') ('volg', 'daarbij') ('daarbij', 'de') ('de', 'uitslag') ('uitslag', 'op') ('op', 'de') ('de', 'voet') ('voet', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'middelbar') ('middelbar', 'schooltijd') ('schooltijd', 'heb') ('heb', 'ik') ('ik', 'vrijwilligerswerk') ('vrijwilligerswerk', 'gedaan') ('gedaan', 'en') ('en', 'ik') ('ik', 'werk') ('werk', 'al') ('al', 'jaren') ('jaren', 'in') ('in', 'een') ('een', 'verzorgingstehui') ('verzorgingstehui', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'studi') ('studi', 'ben') ('ben', 'ik') ('ik', 'actief') ('actief', 'geweest') ('geweest', 'binnen') ('binnen', 'de') ('de', 'faculteitsverenig') ('faculteitsverenig', 'van') ('van', 'de') ('de', 'Universiteit') ('Universiteit', 'van') ('van', 'Tilburg') ('Tilburg', ',') (',', 'in') ('in', 'verschillend') ('verschillend', 'commissi') ('commissi', '.') ('.', 'Met') ('Met', 'anderen') ('anderen', 'heb') ('heb', 'ik') ('i