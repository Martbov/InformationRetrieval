', 'ik') ('ik', 'mij') ('mij', 'in') ('in', 'het') ('het', 'studentenleven') ('studentenleven', 'ingezet') ('ingezet', 'voor') ('voor', 'tal') ('tal', 'van') ('van', 'organisati') ('organisati', 'en') ('en', 'gremia') ('gremia', ',') (',', 'waarond') ('waarond', 'al') ('al', 'bestuurslid') ('bestuurslid', 'van') ('van', 'een') ('een', 'studie-') ('studie-', 'en') ('en', 'van') ('van', 'een') ('een', 'studentenverenig') ('studentenverenig', '.') ('.', 'Ten') ('Ten', 'slott') ('slott', 'ben') ('ben', 'ik') ('ik', 'al') ('al', 'duoraadslid') ('duoraadslid', 'voor') ('voor', 'D66') ('D66', 'werkzaam') ('werkzaam', 'geweest') ('geweest', 'in') ('in', 'de') ('de', 'gemeenteraad') ('gemeenteraad', 'van') ('van', 'Leiden') ('Leiden', '.') ('.', 'De') ('De', 'inhoud') ('inhoud', 'van') ('van', 'deze') ('deze', 'functi') ('functi', 'wa') ('wa', 'heel') ('heel', 'diver') ('diver', ',') (',', 'variërend') ('variërend', 'van') ('van', 'het') ('het', 'deelnemen') ('deelnemen', 'aan') ('aan', 'vergaderingen') ('vergaderingen', 'via') ('via', 'het') ('het', 'bijwonen') ('bijwonen', 'van') ('van', 'maatschappelijk') ('maatschappelijk', 'evenementen') ('evenementen', 'tot') ('tot', 'het') ('het', 'uitdelen') ('uitdelen', 'van') ('van', 'folder') ('folder', 'op') ('op', 'de') ('de', 'markt') ('markt', '.')