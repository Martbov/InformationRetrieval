ar') ('naar', 'dezelfd') ('dezelfd', 'casu') ('casu', '.') ('.', 'Dit') ('Dit', 'maakt') ('maakt', 'mijn') ('mijn', 'studi') ('studi', 'en') ('en', 'het') ('het', 'recht') ('recht', 'erg') ('erg', 'interessant.Het') ('interessant.Het', 'is') ('is', 'belangrijk') ('belangrijk', 'om') ('om', 'kritisch') ('kritisch', 'te') ('te', 'zijn') ('zijn', '.') ('.', 'Ik') ('Ik', 'wil') ('wil', 'vraagteken') ('vraagteken', 'kunnen') ('kunnen', 'zetten') ('zetten', 'bij') ('bij', 'zaken') ('zaken', 'en') ('en', 'oplossingen') ('oplossingen', 'bedenken') ('bedenken', '.') ('.', 'Samenwerken') ('Samenwerken', 'is') ('is', 'erg') ('erg', 'belangrijk') ('belangrijk', ';') (';', 'je') ('je', 'leert') ('leert', 'van') ('van', 'anderen') ('anderen', 'en') ('en', 'daardoor') ('daardoor', 'bereik') ('bereik', 'je') ('je', 'vaak') ('vaak', 'meer') ('meer', '.') ('.', 'Het') ('Het', 'RTP') ('RTP', 'is') ('is', 'een') ('een', 'uitdag') ('uitdag', ',') (',', 'waarin') ('waarin', 'ik') ('ik', 'graag') ('graag', 'mijn') ('mijn', 'best') ('best', 'beentj') ('beentj', 'voor') ('voor', 'wil') ('wil', 'zetten') ('zetten', 'en') ('en', 'voor') ('voor', 'de') ('de', 'voll') ('voll', '100') ('100', '%') ('%', 'wil') ('wil', 'gaan') ('gaan', '!') ('!', 'Ik') ('Ik', 'heb') ('heb', 'het') ('het', 'altijd') ('altijd', 'belangrijk') ('belangrijk', 'gevonden') ('gevonden', 'om') ('om', 'iet') ('iet', 'te') ('te', 'betekenen') ('betekenen', 'voor') ('voor', 'het') ('het', 'groter') ('groter', 'geheel') ('geheel', '.') ('.', 'Achter') ('Achter', 'iedereen') ('iedereen', 'aanlopen') ('aanlopen', 'is') ('is', 'gemakkelijk') ('gemakkelijk', ',') (',', 'maar') ('maar', 'door') ('door', 'samen') ('samen', 'te') ('te', 'werken') ('werken', 'iet') ('iet', 'bereiken') ('bereiken', 'is') ('is', 'een') ('een', 'uitdag') ('uitdag', '.') ('.', 'Op') ('Op', 'de') ('de', 'middelbar') ('middelbar', 'school') ('school', 'zat') ('zat', 'ik') ('ik', 'in') ('in', 'de') ('de', 'leerlingenraad') ('leerlingenraad', 'en') ('en', 'M