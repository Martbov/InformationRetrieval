,') (',', 'en') ('en', 'de') ('de', 'kan') ('kan', 'om') ('om', 'in') ('in', 'verschillend') ('verschillend', 'projecten') ('projecten', 'met') ('met', 'leidinggevend') ('leidinggevend', 'en') ('en', 'inhoudelijk') ('inhoudelijk', 'aspecten') ('aspecten', 'kenni') ('kenni', 'te') ('te', 'maken') ('maken', ',') (',', 'wegen') ('wegen', 'zwaar') ('zwaar', 'voor') ('voor', 'mij') ('mij', '.') ('.', 'Zo') ('Zo', 'vergroot') ('vergroot', 'je') ('je', 'de') ('de', 'kan') ('kan', 'een') ('een', 'functi') ('functi', 'te') ('te', 'vinden') ('vinden', 'die') ('die', 'je') ('je', 'echt') ('echt', 'aanspreekt') ('aanspreekt', '.') ('.', 'Daarnaast') ('Daarnaast', 'spreekt') ('spreekt', 'uit') ('uit', 'deze') ('deze', 'opzet') ('opzet', 'dat') ('dat', 'het') ('het', 'rijk') ('rijk', 'investeert') ('investeert', 'in') ('in', 'haar') ('haar', 'werknem') ('werknem', ',') (',', 'iet') ('iet', 'waar') ('waar', 'ik') ('ik', 'veel') ('veel', 'waard') ('waard', 'aan') ('aan', 'hecht') ('hecht', '.') ('.', 'Mijn') ('Mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'heeft') ('heeft', 'zich') ('zich', 'door') ('door', 'de') ('de', 'jaren') ('jaren', 'heen') ('heen', 'op') ('op', 'verschillend') ('verschillend', 'manieren') ('manieren', 'geuit') ('geuit', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'al') ('al', 'sind') ('sind', 'jong') ('jong', 'leeftijd') ('leeftijd', 'geïnteresseerd') ('geïnteresseerd', 'in') ('in', 'de') ('de', 'politiek') ('politiek', ',') (',', 'dankzij') ('dankzij', 'een') ('een', 'vader') ('vader', 'die') ('die', 'op') ('op', 'provincia') ('provincia', 'niveau') ('niveau', 'politiek') ('politiek', 'actief') ('actief', 'wa') ('wa', '.') ('.', 'Daarnaast') ('Daarnaast', 'deed') ('deed', 'ik') ('ik', 'op') ('op', 'de') ('de', 'middelbar') ('middelbar', 'school') ('school', 'de') ('de', 'eindredacti') ('eindredacti', 'voor') ('voor', 'allerhand') ('allerhand', 'drukwerk') ('drukwerk', ',') (',', 'en') ('en', 'wa') ('wa', 'ik') ('ik', 'werkzaam'