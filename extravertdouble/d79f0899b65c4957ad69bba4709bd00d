at', 'moet') ('moet', 'zijn') ('zijn', 'om') ('om', 'te') ('te', 'waarborgen') ('waarborgen', 'dat') ('dat', 'landen') ('landen', 'al') ('al', 'China') ('China', 'meer') ('meer', 'reken') ('reken', 'gaan') ('gaan', 'houden') ('houden', 'met') ('met', 'arbeidsvoorwaarden') ('arbeidsvoorwaarden', ',') (',', 'milieu') ('milieu', 'etc') ('etc', '.') ('.', 'Het') ('Het', 'is') ('is', 'noodzakelijk') ('noodzakelijk', 'om') ('om', 'nu') ('nu', 'in') ('in', 'te') ('te', 'grijpen') ('grijpen', 'zodat') ('zodat', 'onz') ('onz', 'wereld') ('wereld', 'ook') ('ook', 'voor') ('voor', 'onz') ('onz', 'kleinkinderen') ('kleinkinderen', 'nog') ('nog', 'leefbaar') ('leefbaar', 'is') ('is', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'van') ('van', 'mene') ('mene', 'dat') ('dat', 'hier') ('hier', 'een') ('een', 'duidelijk') ('duidelijk', 'taak') ('taak', 'voor') ('voor', 'de') ('de', 'overheid') ('overheid', 'weggelegd') ('weggelegd', 'is') ('is', '.')