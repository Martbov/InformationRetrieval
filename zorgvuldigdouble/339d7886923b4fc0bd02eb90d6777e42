') ('uw', 'traineeprogramma') ('traineeprogramma', 'een') ('een', 'waardevol') ('waardevol', 'mogelijkheid') ('mogelijkheid', 'biedt') ('biedt', 'tot') ('tot', 'het') ('het', 'ontwikkelen') ('ontwikkelen', 'van') ('van', 'mijn') ('mijn', 'competenti') ('competenti', 'zowel') ('zowel', 'in') ('in', 'de') ('de', 'breedt') ('breedt', 'al') ('al', 'in') ('in', 'de') ('de', 'diept') ('diept', '.') ('.', 'Het') ('Het', 'intensiev') ('intensiev', 'traineeprogramma') ('traineeprogramma', 'zal') ('zal', 'mij') ('mij', 'al') ('al', 'ambitieuz') ('ambitieuz', 'starter') ('starter', 'ten') ('ten', 'slott') ('slott', 'de') ('de', 'mogelijkheid') ('mogelijkheid', 'geven') ('geven', 'het') ('het', 'best') ('best', 'in') ('in', 'mij') ('mij', 'naar') ('naar', 'boven') ('boven', 'te') ('te', 'halen') ('halen', '.') ('.', 'Het') ('Het', 'lijkt') ('lijkt', 'mij') ('mij', 'bijzond') ('bijzond', 'inspirerend') ('inspirerend', 'om') ('om', 'binnen') ('binnen', 'het') ('het', 'Rijk') ('Rijk', 'te') ('te', 'kunnen') ('kunnen', 'werken') ('werken', 'aan') ('aan', 'mijn') ('mijn', 'persoonlijk') ('persoonlijk', 'ontwikkel') ('ontwikkel', 'en') ('en', 'tegelijkertijd') ('tegelijkertijd', 'aan') ('aan', 'een') ('een', 'beter') ('beter', 'samenlev') ('samenlev', 'zowel') ('zowel', 'op') ('op', 'nationa') ('nationa', 'al') ('al', 'internationa') ('internationa', 'niveau') ('niveau', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'reizen') ('reizen', 'en') ('en', 'stage') ('stage', 'in') ('in', 'het') ('het', 'buitenland') ('buitenland', 'heb') ('heb', 'ik') ('ik', 'met') ('met', 'veel') ('veel', 'plezier') ('plezier', 'kenni') ('kenni', 'gemaakt') ('gemaakt', 'met') ('met', 'uiteenlopend') ('uiteenlopend', 'culturen') ('culturen', '.') ('.', 'Ook') ('Ook', 'nam') ('nam', 'ik') ('ik', 'deel') ('deel', 'aan') ('aan', 'Harvard') ('Harvard', 'World') ('World', 'Model') ('Model', 'Unit') ('Unit', 'Nation') ('Nation', 'in') ('in', 'Beij') ('Beij', 'waar') ('waar', 'ik') ('ik', 'de') ('de', 'Verenigd