') ('.', 'Tegenwoordig') ('Tegenwoordig', 'ben') ('ben', 'ik') ('ik', 'actief') ('actief', 'bij') ('bij', 'een') ('een', 'inspraakorgaan') ('inspraakorgaan', 'voor') ('voor', 'allochtonen') ('allochtonen', 'waarin') ('waarin', 'ik') ('ik', 'samen') ('samen', 'met') ('met', 'ander') ('ander', 'jongeren') ('jongeren', 'projecten') ('projecten', 'onderneem') ('onderneem', 'om') ('om', 'de') ('de', 'participati') ('participati', 'van') ('van', 'allochtonen') ('allochtonen', 'in') ('in', 'de') ('de', 'Nederlands') ('Nederlands', 'maatschappij') ('maatschappij', 'te') ('te', 'bevorderen') ('bevorderen', '.') ('.', 'In') ('In', 'het') ('het', 'verleden') ('verleden', 'wa') ('wa', 'ik') ('ik', 'ook') ('ook', 'lid') ('lid', 'van') ('van', 'een') ('een', 'comité') ('comité', 'voor') ('voor', 'de') ('de', 'begeleid') ('begeleid', 'van') ('van', 'allochton') ('allochton', 'kinderen') ('kinderen', 'in') ('in', 'het') ('het', 'onderwij') ('onderwij', '.') ('.', 'Hiernaast') ('Hiernaast', 'volg') ('volg', 'ik') ('ik', 'politiek-bestuurlijk') ('politiek-bestuurlijk', 'ontwikkelingen') ('ontwikkelingen', 'op') ('op', 'de') ('de', 'voet') ('voet', 'en') ('en', 'heb') ('heb', 'ik') ('ik', 'altijd') ('altijd', 'een') ('een', 'progressiev') ('progressiev', 'en') ('en', 'constructiev') ('constructiev', 'mene') ('mene', 'klaarstaan') ('klaarstaan', 'voor') ('voor', 'maatschappelijk') ('maatschappelijk', 'ontwikkelingen') ('ontwikkelingen', '.')