,') (',', 'het') ('het', 'halen') ('halen', 'van') ('van', 'deadlin') ('deadlin', 'en') ('en', 'het') ('het', 'participeren') ('participeren', 'in') ('in', 'verschillend') ('verschillend', 'projecten') ('projecten', '.') ('.', 'Ik') ('Ik', 'wil') ('wil', 'graag') ('graag', 'veel') ('veel', 'betekenen') ('betekenen', 'voor') ('voor', 'Nederland') ('Nederland', ',') (',', 'en') ('en', 'wil') ('wil', 'de') ('de', 'kan') ('kan', 'grijpen') ('grijpen', 'om') ('om', 'door') ('door', 'profession') ('profession', 'te') ('te', 'worden') ('worden', 'opgeleid') ('opgeleid', 'om') ('om', 'binnen') ('binnen', 'het') ('het', 'Rijk') ('Rijk', 'datgen') ('datgen', 'te') ('te', 'doen') ('doen', 'waar') ('waar', 'ik') ('ik', 'go') ('go', 'in') ('in', 'ben') ('ben', '.') ('.', 'Passief') ('Passief', 'betrokken') ('betrokken', '.') ('.', 'Ik') ('Ik', 'volg') ('volg', 'het') ('het', 'nieuw') ('nieuw', 'en') ('en', 'ben') ('ben', 'op') ('op', 'de') ('de', 'hoogt') ('hoogt', 'van') ('van', 'de') ('de', 'nieuwst') ('nieuwst', 'wetsvoorstellen') ('wetsvoorstellen', ',') (',', 'zowel') ('zowel', 'in') ('in', 'Nederland') ('Nederland', 'en') ('en', 'West') ('West', 'Europa') ('Europa', 'al') ('al', 'in') ('in', 'Rusland') ('Rusland', 'en') ('en', 'Oekrain') ('Oekrain', '.') ('.', 'Ook') ('Ook', 'de') ('de', 'recent') ('recent', 'ontikkelingen') ('ontikkelingen', 'in') ('in', 'het') ('het', 'Midden') ('Midden', 'Oosten') ('Oosten', 'hebben') ('hebben', 'mijn') ('mijn', 'aandacht') ('aandacht', 'getrokken') ('getrokken', ',') (',', 'en') ('en', 'ik') ('ik', 'volg') ('volg', 'de') ('de', '(') ('(', 're') ('re', ')') (')', 'acti') ('acti', 'van') ('van', 'de') ('de', 'Europes') ('Europes', 'en') ('en', 'international') ('international', 'instellingen') ('instellingen', 'met') ('met', 'veel') ('veel', 'interess') ('interess', '.') ('.', 'Maar') ('Maar', ',') (',', 'deze') ('deze', 'onderwerpen') ('onderwerpen', 'bespreek') ('bespreek', 'ik') ('ik', 'uitsluitend') ('uitsluitend', 'in') ('in', 'de')