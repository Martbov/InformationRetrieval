bitieu') ('ambitieu', 'en') ('en', 'leergierig') ('leergierig', '.') ('.', 'Al') ('Al', 'deze') ('deze', 'eigenschappen') ('eigenschappen', 'maken') ('maken', 'mij') ('mij', 'een') ('een', 'geschikt') ('geschikt', 'kandidaat') ('kandidaat', 'voor') ('voor', 'het') ('het', 'rijkstraineeship') ('rijkstraineeship', '.') ('.', 'Maatschappelijk') ('Maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'is') ('is', 'voor') ('voor', 'mij') ('mij', 'een') ('een', 'voorwaard') ('voorwaard', 'bij') ('bij', 'het') ('het', 'kiezen') ('kiezen', 'van') ('van', 'een') ('een', 'toekomstig') ('toekomstig', 'baan') ('baan', '.') ('.', 'Gedurend') ('Gedurend', 'mijn') ('mijn', 'studententijd') ('studententijd', 'heb') ('heb', 'ik') ('ik', 'constant') ('constant', 'getracht') ('getracht', 'mijn') ('mijn', 'werkzaamheden') ('werkzaamheden', 'te') ('te', 'verenigen') ('verenigen', 'met') ('met', 'een') ('een', 'zeker') ('zeker', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', '.') ('.', 'Voor') ('Voor', 'de') ('de', 'Nederlands') ('Nederlands', 'Ambassad') ('Ambassad', 'in') ('in', 'Chili') ('Chili', 'heb') ('heb', 'ik') ('ik', 'onderzoek') ('onderzoek', 'gedaan') ('gedaan', 'naar') ('naar', 'de') ('de', 'gezondheidszorg') ('gezondheidszorg', ',') (',', 'ik') ('ik', 'heb') ('heb', '2') ('2', 'maanden') ('maanden', 'vrijwilligerswerk') ('vrijwilligerswerk', 'gedaan') ('gedaan', 'in') ('in', 'Costa') ('Costa', 'Rica') ('Rica', 'en') ('en', 'een') ('een', 'eindscripti') ('eindscripti', 'geschreven') ('geschreven', 'over') ('over', 'duurzaam') ('duurzaam', 'ondernemen') ('ondernemen', '.') ('.', 'Juist') ('Juist', 'de') ('de', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'tilt') ('tilt', 'het') ('het', 'rijkstraineeship') ('rijkstraineeship', 'boven') ('boven', 'ander') ('ander', 'opleidingstrajecten') ('opleidingstrajecten', 'uit') ('uit', '.')