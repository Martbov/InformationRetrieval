eer', ',') (',', 'politie-incidentgegeven') ('politie-incidentgegeven', 'en') ('en', 'de') ('de', 'Leefbaarheidsmonitor') ('Leefbaarheidsmonitor', '.') ('.', 'Daarnaast') ('Daarnaast', 'hebben') ('hebben', 'we') ('we', 'burger') ('burger', 'geinterviewd') ('geinterviewd', '.') ('.', 'Op') ('Op', 'deze') ('deze', 'manier') ('manier', 'konden') ('konden', 'we') ('we', 'o.a') ('o.a', '.') ('.', 'kijken') ('kijken', 'naar') ('naar', 'wat') ('wat', '(') ('(', 'voor') ('voor', 'vervelend') ('vervelend', 'dingen') ('dingen', ')') (')', 'de') ('de', 'burger') ('burger', 'meemaakten') ('meemaakten', 'en') ('en', 'waar') ('waar', 'in') ('in', 'Leiden') ('Leiden', 'de') ('de', 'meest') ('meest', 'incidenten') ('incidenten', 'plaatsvonden') ('plaatsvonden', '.') ('.', 'In') ('In', 'het') ('het', 'weekend') ('weekend', 'werk') ('werk', 'ik') ('ik', 'met') ('met', 'ouderen') ('ouderen', 'op') ('op', 'een') ('een', 'woonafdel') ('woonafdel', 'in') ('in', 'een') ('een', 'psychogeriatrisch') ('psychogeriatrisch', 'verpleeghui') ('verpleeghui', '.') ('.', 'Daar') ('Daar', 'verzorg') ('verzorg', 'ik') ('ik', 'o.a') ('o.a', '.') ('.', 'het') ('het', 'ontbijt') ('ontbijt', ',') (',', 'de') ('de', 'koffi') ('koffi', 'en') ('en', 'thee') ('thee', 'en') ('en', 'het') ('het', 'middageten') ('middageten', '.') ('.', 'Dit') ('Dit', 'werk') ('werk', 'vind') ('vind', 'ik') ('ik', 'heel') ('heel', 'belangrijk') ('belangrijk', 'omdat') ('omdat', 'ik') ('ik', 'vind') ('vind', 'dat') ('dat', 'het') ('het', 'waardevol') ('waardevol', 'is') ('is', 'iet') ('iet', 'voor') ('voor', 'de') ('de', '(') ('(', 'ouder') ('ouder', ')') (')', 'medemen') ('medemen', 'te') ('te', 'kunnen') ('kunnen', 'betekenen') ('betekenen', '.')