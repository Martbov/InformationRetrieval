', 'onder') ('onder', 'meer') ('meer', 'een') ('een', 'jaar') ('jaar', 'lang') ('lang', 'fulltim') ('fulltim', 'voorzitt') ('voorzitt', 'geweest') ('geweest', 'van') ('van', 'de') ('de', 'Juridisch') ('Juridisch', 'Studenten') ('Studenten', 'Verenig') ('Verenig', 'te') ('te', 'Utrecht') ('Utrecht', '(') ('(', 'JSVU') ('JSVU', ')') (')', '.') ('.', 'Daarnaast') ('Daarnaast', 'wa') ('wa', 'ik') ('ik', 'vrijwillig') ('vrijwillig', 'bij') ('bij', 'de') ('de', 'gemeent') ('gemeent', 'Utrecht') ('Utrecht', ',') (',', 'waar') ('waar', 'ik') ('ik', 'voor') ('voor', 'het') ('het', 'zogenaamd') ('zogenaamd', '¿DUO-project¿') ('¿DUO-project¿', 'fungeerd') ('fungeerd', 'al') ('al', 'taalcoach') ('taalcoach', 'voor') ('voor', 'een') ('een', 'nieuw') ('nieuw', 'Nederland') ('Nederland', '.')