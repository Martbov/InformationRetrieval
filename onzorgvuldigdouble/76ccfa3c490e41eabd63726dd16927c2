') ('.', 'In') ('In', 'mijn') ('mijn', 'werk') ('werk', 'heb') ('heb', 'ik') ('ik', 'me') ('me', 'de') ('de', 'laatst') ('laatst', 'jaren') ('jaren', 'gericht') ('gericht', 'op') ('op', 'de') ('de', 'organisati') ('organisati', 'van') ('van', 'programma') ('programma', "'s") ("'s", 'op') ('op', 'universiteiten') ('universiteiten', ',') (',', 'zowel') ('zowel', 'voor') ('voor', 'VWO-scholieren') ('VWO-scholieren', 'al') ('al', 'voor') ('voor', 'international') ('international', 'studenten') ('studenten', '.') ('.', 'Naast') ('Naast', 'mijn') ('mijn', 'werk') ('werk', 'heb') ('heb', 'ik') ('ik', 'binnen') ('binnen', 'de') ('de', 'studieverenig') ('studieverenig', 'voor') ('voor', 'geschiedeni') ('geschiedeni', 'in') ('in', 'Leiden') ('Leiden', 'er') ('er', 'hard') ('hard', 'voor') ('voor', 'gewerkt') ('gewerkt', 'om') ('om', 'meer') ('meer', 'studenten') ('studenten', 'geschiedeni') ('geschiedeni', 'te') ('te', 'betrekken') ('betrekken', 'bij') ('bij', 'de') ('de', 'studieverenig') ('studieverenig', ',') (',', 'onderm') ('onderm', 'door') ('door', 'het') ('het', 'organiseren') ('organiseren', 'van') ('van', 'studiereizen') ('studiereizen', ',') (',', 'een') ('een', 'groot') ('groot', 'symposium') ('symposium', ',') (',', 'activiteiten') ('activiteiten', 'zoal') ('zoal', 'diner') ('diner', 'met') ('met', 'professoren') ('professoren', 'en') ('en', 'rondleidingen') ('rondleidingen', ',') (',', 'uitwisselingen') ('uitwisselingen', 'en') ('en', 'bezoeken') ('bezoeken', 'aan') ('aan', 'instanti') ('instanti', '.') ('.', 'Hierme') ('Hierme', 'heb') ('heb', 'ik') ('ik', 'de') ('de', 'studenten') ('studenten', 'proberen') ('proberen', 'uit') ('uit', 'te') ('te', 'dagen') ('dagen', 'om') ('om', 'meer') ('meer', 'uit') ('uit', 'hun') ('hun', 'studi') ('studi', 'te') ('te', 'halen') ('halen', '.') ('.', 'Ook') ('Ook', 'ben') ('ben', 'ik') ('ik', 'actief') ('actief', 'geweest') ('geweest', 'binnen') ('binnen', 'de') ('de', 'studentenvertegenwoordig') ('studentenvertegenwoordig', 