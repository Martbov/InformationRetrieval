f') ('actief', 'geweest') ('geweest', 'voor') ('voor', 'de') ('de', 'Studentenorganisati') ('Studentenorganisati', 'Groningen') ('Groningen', '(') ('(', 'SOG') ('SOG', ')') (')', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'me') ('me', 'een') ('een', 'jaar') ('jaar', 'lang') ('lang', 'ingezet') ('ingezet', 'voor') ('voor', 'de') ('de', 'belangen') ('belangen', 'van') ('van', 'studenten') ('studenten', 'in') ('in', 'onz') ('onz', 'Universiteitsraad') ('Universiteitsraad', '.') ('.', 'Daarnaast') ('Daarnaast', 'wa') ('wa', 'ik') ('ik', 'voorzitt') ('voorzitt', 'van') ('van', 'de') ('de', 'Universitair') ('Universitair', 'Commissi') ('Commissi', 'Studentenbeleid') ('Studentenbeleid', 'en') ('en', 'van') ('van', 'Spiritu') ('Spiritu', ',') (',', 'de') ('de', 'cultuurcommissi') ('cultuurcommissi', 'van') ('van', 'mijn') ('mijn', 'studentenverenig') ('studentenverenig', ',') (',', 'waar') ('waar', 'ik') ('ik', 'een') ('een', 'aantal') ('aantal', 'jaar') ('jaar', 'erg') ('erg', 'actief') ('actief', 'ben') ('ben', 'geweest') ('geweest', '.') ('.', 'In') ('In', 'mijn') ('mijn', 'Master') ('Master', 'heb') ('heb', 'ik') ('ik', 'mij') ('mij', 'toegespitst') ('toegespitst', 'op') ('op', 'ontwikkelingssamenwerk') ('ontwikkelingssamenwerk', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'stage') ('stage', 'gelopen') ('gelopen', 'voor') ('voor', 'het') ('het', 'Unit') ('Unit', 'Nation') ('Nation', 'Develop') ('Develop', 'Programm') ('Programm', 'in') ('in', 'Rwanda') ('Rwanda', '.') ('.', 'Mijn') ('Mijn', 'huidig') ('huidig', 'stage') ('stage', 'in') ('in', 'Kosovo') ('Kosovo', 'spitst') ('spitst', 'zich') ('zich', 'toe') ('toe', 'op') ('op', 'het') ('het', 'Nederlands') ('Nederlands', 'ontwikkelingsbeleid') ('ontwikkelingsbeleid', '.') ('.', 'Ik') ('Ik', 'houd') ('houd', 'me') ('me', 'bezig') ('bezig', 'met') ('met', 'het') ('het', 'beoordelen') ('beoordelen', 'en') ('en', 'monitoren') ('monitoren', 'van') ('van', 'de') ('de', 'klein') ('klein', 'ambassadeprojecten') ('ambassadeprojecten', '.