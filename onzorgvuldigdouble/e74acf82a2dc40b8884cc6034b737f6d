') ('in', 'Den') ('Den', 'Haag') ('Haag', 'te') ('te', 'vertegenwoordigen') ('vertegenwoordigen', '.') ('.', 'Momenteel') ('Momenteel', 'zit') ('zit', 'ik') ('ik', 'in') ('in', 'het') ('het', 'damesbestuur') ('damesbestuur', 'van') ('van', 'mijn') ('mijn', 'voetbalclub') ('voetbalclub', ',') (',', 'gelegen') ('gelegen', 'in') ('in', 'de') ('de', 'Utrechts') ('Utrechts', '¿Vogelaarwijk¿') ('¿Vogelaarwijk¿', 'Kanaleneiland') ('Kanaleneiland', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'verantwoordelijk') ('verantwoordelijk', 'voor') ('voor', 'een') ('een', 'groep') ('groep', 'Marokkaans') ('Marokkaans', 'meiden') ('meiden', 'die') ('die', 'onder') ('onder', 'onz') ('onz', 'vlag') ('vlag', 'competiti') ('competiti', 'speelt') ('speelt', '.') ('.', 'Zo') ('Zo', 'zie') ('zie', 'ik') ('ik', 'met') ('met', 'eigen') ('eigen', 'ogen') ('ogen', 'dat') ('dat', 'sport') ('sport', 'integreert') ('integreert', '!') ('!', 'Uiteraard') ('Uiteraard', 'vind') ('vind', 'ik') ('ik', 'het') ('het', 'verder') ('verder', 'belangrijk') ('belangrijk', 'de') ('de', 'krant') ('krant', 'te') ('te', 'lezen') ('lezen', 'en') ('en', 'debatten') ('debatten', 'te') ('te', 'bezoeken') ('bezoeken', '.') ('.', 'Voor') ('Voor', 'mijn') ('mijn', 'studi') ('studi', 'heb') ('heb', 'ik') ('ik', 'tot') ('tot', 'nu') ('nu', 'toe') ('toe', 'een') ('een', 'aantal') ('aantal', 'keren') ('keren', 'met') ('met', 'overheidsbeleid') ('overheidsbeleid', 'kenni') ('kenni', 'gemaakt') ('gemaakt', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'studi') ('studi', 'onderzocht') ('onderzocht', 'ik') ('ik', 'voor') ('voor', 'het') ('het', 'NRC') ('NRC', 'het') ('het', 'asielbeleid') ('asielbeleid', ',') (',', 'schreef') ('schreef', 'ik') ('ik', 'een') ('een', 'scripti') ('scripti', 'over') ('over', 'burgemeestersverkiezingen') ('burgemeestersverkiezingen', 'en') ('en', 'dit') ('dit', 'jaar') ('jaar', 'een') ('een', 'paper') ('paper', 'over') ('over', 'het') ('het', '¿collectief') ('¿collectief', 'actieprobleem¿') ('actieprob