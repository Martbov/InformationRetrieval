,') (',', 'studi') ('studi', 'in') ('in', 'Milaan') ('Milaan', 'en') ('en', 'het') ('het', 'UN') ('UN', 'WFP') ('WFP', 'in') ('in', 'Swaziland') ('Swaziland', '.') ('.', 'Kort') ('Kort', 'gezegd') ('gezegd', 'kan') ('kan', 'binnen') ('binnen', 'dit') ('dit', 'traineeship') ('traineeship', 'een') ('een', 'win-win') ('win-win', 'situati') ('situati', 'ontstaan') ('ontstaan', '.') ('.', 'Ik') ('Ik', 'zou') ('zou', 'mijn') ('mijn', 'kwaliteiten') ('kwaliteiten', 'kunnen') ('kunnen', 'gebruiken') ('gebruiken', 'en') ('en', 'uitbreiden') ('uitbreiden', '.') ('.', 'Het') ('Het', 'rijk') ('rijk', 'zal') ('zal', 'aan') ('aan', 'mij') ('mij', 'een') ('een', 'waardevol') ('waardevol', ',') (',', 'hands-on') ('hands-on', 'en') ('en', 'gemotiveerd') ('gemotiveerd', 'traine') ('traine', 'hebben') ('hebben', '.') ('.', 'Ik') ('Ik', 'hoop') ('hoop', 'dan') ('dan', 'ook') ('ook', 'deel') ('deel', 'te') ('te', 'nemen') ('nemen', 'aan') ('aan', 'dit') ('dit', 'programma') ('programma', '.') ('.', 'Flori') ('Flori', 'de') ('de', 'Roy') ('Roy', 'van') ('van', 'Zuydewijn') ('Zuydewijn', 'Qua') ('Qua', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'heb') ('heb', 'ik') ('ik', 'mij') ('mij', 'altijd') ('altijd', 'met') ('met', 'veel') ('veel', 'interess') ('interess', 'en') ('en', 'plezier') ('plezier', 'actief') ('actief', 'opgesteld') ('opgesteld', 'in') ('in', 'mijn') ('mijn', 'omgev') ('omgev', '.') ('.', 'Op') ('Op', 'de') ('de', 'middelbar') ('middelbar', 'school') ('school', 'wa') ('wa', 'ik') ('ik', 'actief') ('actief', 'al') ('al', 'voorzitt') ('voorzitt', 'van') ('van', 'de') ('de', 'leerlingenverenig') ('leerlingenverenig', 'en') ('en', 'al') ('al', 'trainer/coach') ('trainer/coach', 'op') ('op', 'het') ('het', 'hockeyveld') ('hockeyveld', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'studi') ('studi', 'ben') ('ben', 'ik') ('ik', 'betrokken') ('betrokken', 'geweest') ('geweest', 'bij') ('bij', 'onder') ('onder', 'ander') ('ander', 'de') ('de', 'Rag