') ('de', 'juist') ('juist', 'vragen') ('vragen', 'te') ('te', 'stellen') ('stellen', '.') ('.', 'Ik') ('Ik', 'wil') ('wil', 'weten') ('weten', 'wat') ('wat', 'de') ('de', 'achtergrond') ('achtergrond', 'van') ('van', 'bepaald') ('bepaald', 'zaken') ('zaken', 'is') ('is', 'en') ('en', 'welk') ('welk', 'maatschappelijk') ('maatschappelijk', 'vraagstuk') ('vraagstuk', 'achter') ('achter', 'de') ('de', 'casu') ('casu', 'schuil') ('schuil', 'gaat') ('gaat', '.') ('.', 'Het') ('Het', 'intrigeert') ('intrigeert', 'mij') ('mij', 'waarom') ('waarom', 'bepaald') ('bepaald', 'wetgev') ('wetgev', 'tot') ('tot', 'stand') ('stand', 'is') ('is', 'gekomen') ('gekomen', 'en') ('en', 'het') ('het', 'daarvoor') ('daarvoor', 'ontwikkeld') ('ontwikkeld', 'beleid') ('beleid', 'te') ('te', 'analyseren') ('analyseren', '.') ('.', 'Mijn') ('Mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'heeft') ('heeft', 'erto') ('erto', 'geleid') ('geleid', 'dat') ('dat', 'ik') ('ik', 'Nederland') ('Nederland', 'Recht') ('Recht', 'ben') ('ben', 'gaan') ('gaan', 'studeren') ('studeren', '.') ('.', 'Vakken') ('Vakken', 'met') ('met', 'een') ('een', 'maatschappelijk') ('maatschappelijk', 'karakt') ('karakt', 'hebben') ('hebben', 'altijd') ('altijd', 'mijn') ('mijn', 'bijzonder') ('bijzonder', 'aandacht') ('aandacht', 'getrokken') ('getrokken', '.') ('.', 'Dit') ('Dit', 'blijkt') ('blijkt', 'ook') ('ook', 'uit') ('uit', 'de') ('de', 'keuz') ('keuz', 'voor') ('voor', 'mijn') ('mijn', 'keuzevakken') ('keuzevakken', 'en') ('en', 'master') ('master', 'straf-') ('straf-', 'en') ('en', 'arbeidsrecht') ('arbeidsrecht', '.') ('.', 'Mijn') ('Mijn', 'maatschappelijk') ('maatschappelijk', 'interess') ('interess', 'blijkt') ('blijkt', 'ook') ('ook', 'uit') ('uit', 'mijn') ('mijn', 'scripti') ('scripti', ',') (',', 'waarin') ('waarin', 'ik') ('ik', 'met') ('met', 'veel') ('veel', 'enthousiasm') ('enthousiasm', 'heb') ('heb', 'onderzocht') ('onderzocht', 'of') ('of', 'vrouwen') ('vrouwen'