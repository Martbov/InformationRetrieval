', 'In') ('In', 'India') ('India', 'heb') ('heb', 'ik') ('ik', 'in') ('in', 'samenwerk') ('samenwerk', 'met') ('met', 'een') ('een', 'NGO') ('NGO', '(') ('(', 'Non') ('Non', 'Government') ('Government', 'Organ') ('Organ', ')') (')', 'een') ('een', 'kindertehui') ('kindertehui', 'opgezet') ('opgezet', '.') ('.', 'De') ('De', 'bouw') ('bouw', 'van') ('van', 'het') ('het', 'kindertehui') ('kindertehui', 'is') ('is', 'gefinancierd') ('gefinancierd', 'door') ('door', 'persoonlijk') ('persoonlijk', 'inzamelingsacti') ('inzamelingsacti', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'daar') ('daar', 'vier') ('vier', 'maanden') ('maanden', 'vrijwilligerswerk') ('vrijwilligerswerk', 'met') ('met', 'verstandelijk') ('verstandelijk', 'en/of') ('en/of', 'lichamelijk') ('lichamelijk', 'gehandicapt') ('gehandicapt', 'kinderen') ('kinderen', 'gedaan') ('gedaan', '.') ('.', 'Nog') ('Nog', 'steed') ('steed', 'geef') ('geef', 'ik') ('ik', 'voorlicht') ('voorlicht', 'aan') ('aan', 'vrijwillig') ('vrijwillig', 'en') ('en', 'stagiair') ('stagiair', '.') ('.', 'Ik') ('Ik', 'werk') ('werk', 'in') ('in', 'de') ('de', 'kinderpsychiatri') ('kinderpsychiatri', '.') ('.', 'Ik') ('Ik', 'woon') ('woon', 'op') ('op', 'een') ('een', 'hofj') ('hofj', 'waar') ('waar', 'ik') ('ik', 'ook') ('ook', 'beheerd') ('beheerd', 'ben') ('ben', 'en') ('en', 'een') ('een', 'social') ('social', 'functi') ('functi', 'heb') ('heb', 'op') ('op', 'het') ('het', 'hofj') ('hofj', '.')