') ('onderzoeken', '.') ('.', 'In') ('In', 'mijn') ('mijn', 'opleid') ('opleid', 'is') ('is', 'het') ('het', 'van') ('van', 'belang') ('belang', 'geweest') ('geweest', 'om') ('om', 'kritisch') ('kritisch', 'te') ('te', 'kijken') ('kijken', 'naar') ('naar', 'de') ('de', 'verschillend') ('verschillend', 'vraagstukken') ('vraagstukken', 'en') ('en', 'verschillend') ('verschillend', 'zienswijzen') ('zienswijzen', 'te') ('te', 'herkennen') ('herkennen', '.') ('.', 'Het') ('Het', 'wa') ('wa', 'van') ('van', 'belang') ('belang', 'om') ('om', 'deze') ('deze', 'in') ('in', 'overweg') ('overweg', 'te') ('te', 'kunnen') ('kunnen', 'nemen') ('nemen', 'om') ('om', 'zo') ('zo', 'tot') ('tot', 'een') ('een', 'weloverwogen') ('weloverwogen', 'en') ('en', 'goed') ('goed', 'conclusi') ('conclusi', 'te') ('te', 'komen') ('komen', '.') ('.', 'Naast') ('Naast', 'mijn') ('mijn', 'studi') ('studi', 'heb') ('heb', 'ik') ('ik', 'mij') ('mij', 'ingezet') ('ingezet', 'bij') ('bij', 'verschillend') ('verschillend', 'verenigingen') ('verenigingen', 'waar') ('waar', 'ik') ('ik', 'op') ('op', 'bestuurlijk') ('bestuurlijk', 'en') ('en', 'organisatorisch') ('organisatorisch', 'niveau') ('niveau', 'ervar') ('ervar', 'heb') ('heb', 'op') ('op', 'gedaan') ('gedaan', 'door') ('door', 'het') ('het', 'bekleden') ('bekleden', 'van') ('van', 'bestuursfuncti') ('bestuursfuncti', ',') (',', 'door') ('door', 'zit') ('zit', 'te') ('te', 'nemen') ('nemen', 'in') ('in', 'commissi') ('commissi', 'en') ('en', 'door') ('door', 'het') ('het', 'coachen') ('coachen', 'van') ('van', 'team') ('team', '.')