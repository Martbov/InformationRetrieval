ar') ('jaar', 'ga') ('ga', 'ik') ('ik', 'mee') ('mee', 'met') ('met', 'de') ('de', 'roparun') ('roparun', 'om') ('om', 'geld') ('geld', 'in') ('in', 'te') ('te', 'zamelen') ('zamelen', 'voor') ('voor', 'mensen') ('mensen', 'met') ('met', 'kanker') ('kanker', '.') ('.', 'on') ('on', 'team') ('team', 'heeft') ('heeft', 'al') ('al', 'ruim') ('ruim', '60.000') ('60.000', 'euro') ('euro', 'over') ('over', 'kunnen') ('kunnen', 'maken') ('maken', 'aan') ('aan', 'de') ('de', 'sticht') ('sticht', '.')