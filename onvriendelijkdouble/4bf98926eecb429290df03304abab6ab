id') ('opleid', '.') ('.', 'Beid') ('Beid', 'opleidingen') ('opleidingen', 'heb') ('heb', 'ik') ('ik', 'cum') ('cum', 'laud') ('laud', 'afgerond') ('afgerond', '.') ('.', 'Bij') ('Bij', 'Halt') ('Halt', 'Oost') ('Oost', 'Brabant') ('Brabant', 'is') ('is', 'het') ('het', 'ten') ('ten', 'slott') ('slott', 'zaak') ('zaak', 'om') ('om', 'zo') ('zo', 'snel') ('snel', 'mogelijk') ('mogelijk', 'te') ('te', 'reageren') ('reageren', 'en') ('en', 'in') ('in', 'te') ('te', 'springen') ('springen', 'op') ('op', 'problemen') ('problemen', 'van') ('van', 'bijvoorbeeld') ('bijvoorbeeld', 'cliënten') ('cliënten', 'of') ('of', 'scholen') ('scholen', '.') ('.', 'Resultaatgerichtheid') ('Resultaatgerichtheid', 'is') ('is', 'daarbij') ('daarbij', 'van') ('van', 'groot') ('groot', 'belang') ('belang', '.') ('.', 'Mijn') ('Mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'uit') ('uit', 'zich') ('zich', 'in') ('in', 'eerst') ('eerst', 'instanti') ('instanti', 'in') ('in', 'de') ('de', 'belangstel') ('belangstel', 'die') ('die', 'ik') ('ik', 'heb') ('heb', 'in') ('in', 'wat') ('wat', 'er') ('er', 'in') ('in', 'het') ('het', 'land') ('land', 'en') ('en', 'in') ('in', 'de') ('de', 'wereld') ('wereld', 'gebeurt') ('gebeurt', '.') ('.', 'Dit') ('Dit', 'uit') ('uit', 'zich') ('zich', 'in') ('in', 'het') ('het', 'kijken') ('kijken', 'van') ('van', 'het') ('het', 'nieuw') ('nieuw', ',') (',', 'het') ('het', 'lezen') ('lezen', 'van') ('van', 'kranten') ('kranten', 'en') ('en', 'het') ('het', 'volgen') ('volgen', 'van') ('van', 'actualiteitenprogramma¿') ('actualiteitenprogramma¿', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'nieuwsgierig') ('nieuwsgierig', 'met') ('met', 'betrek') ('betrek', 'tot') ('tot', 'de') ('de', 'gebeurtenissen') ('gebeurtenissen', 'die') ('die', 'plaatsvinden') ('plaatsvinden', 'en') ('en', 'de') ('de', 'acti') ('acti', 'die') ('die', 'daaropvolgend') ('daaropvolgend', 'worden') ('worden', 'genomen') ('genomen', '.') ('.', 'Ook') ('Ook', 'op') ('