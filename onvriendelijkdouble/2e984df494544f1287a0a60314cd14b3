et') ('het', 'leidingteam') ('leidingteam', ',') (',', 'waarbij') ('waarbij', 'ik') ('ik', 'onder') ('onder', 'ander') ('ander', 'zomerkampen') ('zomerkampen', 'verzorg') ('verzorg', 'voor') ('voor', 'ca') ('ca', '.') ('.', '30') ('30', 'jongeren') ('jongeren', ',') (',', 'een') ('een', 'jaarlijks') ('jaarlijks', 'dodenherdenk') ('dodenherdenk', 'op') ('op', 'vier') ('vier', 'mei') ('mei', 'in') ('in', 'Haarlem') ('Haarlem', 'met') ('met', 'de') ('de', 'burgemeest') ('burgemeest', 'en') ('en', 'me') ('me', 'inzet') ('inzet', 'voor') ('voor', 'de') ('de', 'ontwikkel') ('ontwikkel', 'van') ('van', 'de') ('de', 'jongen') ('jongen', 'die') ('die', 'lid') ('lid', 'zijn') ('zijn', 'in') ('in', 'de') ('de', 'groep') ('groep', '.') ('.', 'Er') ('Er', 'zijn') ('zijn', 'meerder') ('meerder', 'activiteiten') ('activiteiten', 'per') ('per', 'jaar') ('jaar', 'met') ('met', 'ander') ('ander', 'groepen') ('groepen', 'en') ('en', 'ander') ('ander', 'regio') ('regio', "'s") ("'s", 'en') ('en', 'zo') ('zo', 'ontmoet') ('ontmoet', 'je') ('je', 'een') ('een', 'een') ('een', 'hoop') ('hoop', 'ander') ('ander', 'mensen') ('mensen', 'en') ('en', 'denkwijzen') ('denkwijzen', '.') ('.', 'Ook') ('Ook', 'lopen') ('lopen', 'wij') ('wij', 'elk') ('elk', 'jaar') ('jaar', 'de') ('de', 'Jantj') ('Jantj', 'Beton') ('Beton', 'collect') ('collect', 'om') ('om', 'on') ('on', 'steentj') ('steentj', 'bij') ('bij', 'te') ('te', 'dragen') ('dragen', '.') ('.', 'Hiernaast') ('Hiernaast', 'ben') ('ben', 'ik') ('ik', 'ook') ('ook', 'betrokken') ('betrokken', 'bij') ('bij', 'Cafe') ('Cafe', 'Politiek') ('Politiek', 'en') ('en', 'Radio') ('Radio', 'Politiek') ('Politiek', '.') ('.', 'Bij') ('Bij', 'Cafe') ('Cafe', 'Politiek') ('Politiek', 'vinden') ('vinden', 'er') ('er', 'regelmatig') ('regelmatig', 'debatten') ('debatten', 'plaat') ('plaat', 'met') ('met', 'politici') ('politici', 'ed') ('ed', '.') ('.', 'Radio') ('Radio', 'Politiek') ('Politiek', 'is') ('is', 'een') ('een', 'wekelijk') ('wekelijk', 'uurtj')