') ('houd', 'mij') ('mij', 'actief') ('actief', 'betrokken') ('betrokken', 'in') ('in', 'de') ('de', 'maatschappij') ('maatschappij', 'd.m.v') ('d.m.v', '.') ('.', 'lidmaatschappen') ('lidmaatschappen', 'van') ('van', 'een') ('een', 'sportclub') ('sportclub', ',') (',', 'studentenverenig') ('studentenverenig', ',') (',', 'kerk') ('kerk', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'een') ('een', 'jaar') ('jaar', 'dispuutvoorzitt') ('dispuutvoorzitt', 'geweest') ('geweest', 'binnen') ('binnen', 'mijn') ('mijn', 'studentenverenig') ('studentenverenig', 'en') ('en', 'heb') ('heb', 'daar') ('daar', 'in') ('in', 'overleg') ('overleg', 'en') ('en', 'samenwerkend') ('samenwerkend', 'gezorgd') ('gezorgd', 'dat') ('dat', 'we') ('we', 'een') ('een', 'levendig') ('levendig', 'dispuut') ('dispuut', 'hadden') ('hadden', '.') ('.', 'Ik') ('Ik', 'houd') ('houd', 'van') ('van', 'vernieuw') ('vernieuw', 'en') ('en', 'anderen') ('anderen', 'daarin') ('daarin', 'meenemen') ('meenemen', '.') ('.', 'Voor') ('Voor', 'de') ('de', 'kerk') ('kerk', 'heb') ('heb', 'ik') ('ik', 'vier') ('vier', 'keer') ('keer', 'een') ('een', 'kamp') ('kamp', 'georganiseerd') ('georganiseerd', '.') ('.', 'Ook') ('Ook', 'ondersteun') ('ondersteun', 'ik') ('ik', 'divers') ('divers', 'goed') ('goed', 'doelen') ('doelen', ',') (',', 'zowel') ('zowel', 'financieel') ('financieel', 'al') ('al', 'door') ('door', 'het') ('het', 'geven') ('geven', 'van') ('van', 'mijn') ('mijn', 'tijd') ('tijd', '.')