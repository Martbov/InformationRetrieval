uderen') ('studeren', '.') ('.', 'Daar') ('Daar', 'heb') ('heb', 'ik') ('ik', 'ook') ('ook', 'veel') ('veel', 'geleerd') ('geleerd', 'over') ('over', 'ander') ('ander', 'culturen') ('culturen', 'en') ('en', 'maatschappijen') ('maatschappijen', '.') ('.', 'Ik') ('Ik', 'zie') ('zie', 'werken') ('werken', 'bij') ('bij', 'het') ('het', 'Rijk') ('Rijk', 'al') ('al', 'een') ('een', 'geweldig') ('geweldig', 'kan') ('kan', 'om') ('om', 'een') ('een', 'bijdrag') ('bijdrag', 'te') ('te', 'leveren') ('leveren', 'aan') ('aan', 'de') ('de', 'maatschappij') ('maatschappij', 'en') ('en', 'mijzelf') ('mijzelf', 'verder') ('verder', 'te') ('te', 'ontwikkelen') ('ontwikkelen', '.') ('.', 'Wat') ('Wat', 'er') ('er', 'in') ('in', 'de') ('de', 'wereld') ('wereld', 'en') ('en', 'om') ('om', 'me') ('me', 'heen') ('heen', 'gebeurt') ('gebeurt', ',') (',', 'is') ('is', 'belangrijk') ('belangrijk', 'voor') ('voor', 'mij') ('mij', '.') ('.', 'Dagelijk') ('Dagelijk', 'houd') ('houd', 'ik') ('ik', 'me') ('me', 'op') ('op', 'de') ('de', 'hoogt') ('hoogt', 'via') ('via', 'kranten') ('kranten', 'en') ('en', 'het') ('het', 'internet') ('internet', '.') ('.', 'Van') ('Van', 'jong') ('jong', 'af') ('af', 'aan') ('aan', 'ben') ('ben', 'ik') ('ik', 'erg') ('erg', 'actief') ('actief', 'geweest') ('geweest', 'in') ('in', 'de') ('de', 'sportwereld') ('sportwereld', ',') (',', 'voornamelijk') ('voornamelijk', 'in') ('in', 'hockey') ('hockey', 'en') ('en', 'cricket') ('cricket', '.') ('.', 'Later') ('Later', 'in') ('in', 'mijn') ('mijn', 'leven') ('leven', 'heeft') ('heeft', 'cricket') ('cricket', 'de') ('de', 'overhand') ('overhand', 'genomen') ('genomen', '.') ('.', 'Bij') ('Bij', 'mijn') ('mijn', 'cricket') ('cricket', 'club') ('club', '(') ('(', 'HV') ('HV', '&') ('&', 'CV') ('CV', 'QUICK') ('QUICK', ')') (')', 'ben') ('ben', 'ik') ('ik', 'actief') ('actief', 'met') ('met', 'trainen') ('trainen', 'van') ('van', 'jeugd') ('jeugd', 'en') ('en', 'het') ('het', 'trainen') ('trainen', 'van') ('van', 'het') (