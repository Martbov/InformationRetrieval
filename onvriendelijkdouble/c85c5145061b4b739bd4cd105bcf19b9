oet') ('moet', 'toch') ('toch', 'ook') ('ook', 'beter') ('beter', 'kunnen') ('kunnen', "'") ("'", '.') ('.', 'Met') ('Met', 'een') ('een', 'rijkstraineeship') ('rijkstraineeship', 'denk') ('denk', 'ik') ('ik', 'dat') ('dat', 'ik') ('ik', 'een') ('een', 'go') ('go', 'beeld') ('beeld', 'krijg') ('krijg', 'van') ('van', 'de') ('de', 'overheid') ('overheid', '.') ('.', 'Vanuit') ('Vanuit', 'die') ('die', 'positi') ('positi', 'kan') ('kan', 'ik') ('ik', 'mee') ('mee', 'gaan') ('gaan', 'werken') ('werken', 'aan') ('aan', 'verbeteringen') ('verbeteringen', 'om') ('om', 'on') ('on', 'land') ('land', 'ook') ('ook', 'in') ('in', 'de') ('de', 'aankomend') ('aankomend', 'decennia') ('decennia', 'go') ('go', 'voor') ('voor', 'de') ('de', 'dag') ('dag', 'te') ('te', 'laten') ('laten', 'komen') ('komen', '.') ('.', 'Maatschappelijk') ('Maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'is') ('is', 'duidelijk') ('duidelijk', 'bij') ('bij', 'mij') ('mij', 'aanwezig') ('aanwezig', '.') ('.', 'Ik') ('Ik', 'voel') ('voel', 'me') ('me', 'go') ('go', 'al') ('al', 'mensen') ('mensen', 'om') ('om', 'mij') ('mij', 'heen') ('heen', 'het') ('het', 'ook') ('ook', 'go') ('go', 'hebben') ('hebben', '.') ('.', 'Dit') ('Dit', 'uit') ('uit', 'zich') ('zich', 'niet') ('niet', 'in') ('in', 'een') ('een', 'kleur') ('kleur', 'die') ('die', 'standaard') ('standaard', 'bij') ('bij', 'een') ('een', 'politiek') ('politiek', 'partij') ('partij', 'of') ('of', 'beweg') ('beweg', 'past') ('past', '.') ('.', 'Het') ('Het', 'is') ('is', 'meer') ('meer', 'op') ('op', 'een') ('een', 'persoonlijk') ('persoonlijk', 'niveau') ('niveau', '.') ('.', 'Ik') ('Ik', 'volg') ('volg', 'de') ('de', 'actualiteiten') ('actualiteiten', 'dagelijk') ('dagelijk', '.') ('.', 'Zorg') ('Zorg', 'dat') ('dat', 'daar') ('daar', 'waar') ('waar', 'ik') ('ik', 'mensen') ('mensen', 'zou') ('zou', 'kunnen') ('kunnen', 'helpen') ('helpen', ',') (',', 'ik') ('ik', 'dat') ('dat', 'ook') ('ook', 'doe') ('doe', ',') (',', 'denk') ('denk', 'aan'