ote') ('grote', 'interess') ('interess', 'de') ('de', 'actualiteiten') ('actualiteiten', 'in') ('in', 'het') ('het', 'nieuw') ('nieuw', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'dan') ('dan', 'ook') ('ook', 'altijd') ('altijd', 'op') ('op', 'de') ('de', 'hoogt') ('hoogt', 'van') ('van', 'wat') ('wat', 'er') ('er', 'speelt') ('speelt', 'in') ('in', 'Nederland') ('Nederland', 'en') ('en', 'de') ('de', 'rest') ('rest', 'van') ('van', 'de') ('de', 'wereld') ('wereld', '.') ('.', 'Verder') ('Verder', 'uit') ('uit', 'mijn') ('mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'zich') ('zich', 'ook') ('ook', 'in') ('in', 'de') ('de', 'praktijk') ('praktijk', '.') ('.', 'Al') ('Al', 'bijna') ('bijna', 'tien') ('tien', 'jaar') ('jaar', 'zet') ('zet', 'ik') ('ik', 'mij') ('mij', 'in') ('in', 'al') ('al', 'vrijwillig') ('vrijwillig', 'op') ('op', 'mijn') ('mijn', 'tennisverenig') ('tennisverenig', '.') ('.', 'Zo') ('Zo', 'zit') ('zit', 'ik') ('ik', 'onder') ('onder', 'ander') ('ander', 'in') ('in', 'de') ('de', 'Jeugdcommissi') ('Jeugdcommissi', 'en') ('en', 'houd') ('houd', 'ik') ('ik', 'mij') ('mij', 'du') ('du', 'bezig') ('bezig', 'met') ('met', 'het') ('het', 'jeugdbeleid') ('jeugdbeleid', 'van') ('van', 'de') ('de', 'verenig') ('verenig', '.') ('.', 'In') ('In', 'mijn') ('mijn', 'woonplaat') ('woonplaat', 'Leiden') ('Leiden', 'zet') ('zet', 'ik') ('ik', 'mij') ('mij', 'al') ('al', 'jaren') ('jaren', 'in') ('in', 'op') ('op', 'cultureel') ('cultureel', 'gebi') ('gebi', '.') ('.', 'Samen') ('Samen', 'met') ('met', 'vrienden') ('vrienden', 'heb') ('heb', 'ik') ('ik', 'het') ('het', 'Leid') ('Leid', 'Film') ('Film', 'Festiv') ('Festiv', 'opgericht') ('opgericht', 'om') ('om', 'Leiden') ('Leiden', 'op') ('op', 'cultureel') ('cultureel', 'gebi') ('gebi', 'weer') ('weer', 'op') ('op', 'de') ('de', 'kaart') ('kaart', 'te') ('te', 'zetten') ('zetten', '.') ('.', 'Dit') ('Dit', 'is') ('is', 'aardig') ('aardig', 'gelukt') ('gelukt', ',') (',', 'want') ('w