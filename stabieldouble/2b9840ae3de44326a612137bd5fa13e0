', 'georganiseerd') ('georganiseerd', '.') ('.', 'Daarnaast') ('Daarnaast', 'geef') ('geef', 'ik') ('ik', 'sind') ('sind', '2004') ('2004', 'bijl') ('bijl', 'en') ('en', 'examentrainingen') ('examentrainingen', 'wiskund') ('wiskund', 'aan') ('aan', 'middelbar') ('middelbar', 'scholieren') ('scholieren', 'van') ('van', 'allerlei') ('allerlei', 'niveau') ('niveau', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'in') ('in', 'die') ('die', 'period') ('period', 'ook') ('ook', 'een') ('een', 'jaar') ('jaar', 'op') ('op', 'een') ('een', 'school') ('school', 'lesgegeven') ('lesgegeven', '.') ('.', 'Verder') ('Verder', 'werk') ('werk', 'ik') ('ik', 'via') ('via', 'het') ('het', 'Nederland') ('Nederland', 'Centrum') ('Centrum', 'voor') ('voor', 'Total') ('Total', 'Toegankelijkheid') ('Toegankelijkheid', 'mee') ('mee', 'aan') ('aan', 'het') ('het', 'in') ('in', 'kaart') ('kaart', 'brengen') ('brengen', 'en') ('en', 'verbeteren') ('verbeteren', 'van') ('van', 'fysiek') ('fysiek', 'en') ('en', 'mental') ('mental', 'toegankelijkheid') ('toegankelijkheid', 'voor') ('voor', 'mensen') ('mensen', 'met') ('met', 'een') ('een', 'functiebeperk') ('functiebeperk', '.') ('.', 'Ook') ('Ook', 'heb') ('heb', 'ik') ('ik', 'lang') ('lang', 'tijd') ('tijd', 'voetbaltrain') ('voetbaltrain', 'gegeven') ('gegeven', 'aan') ('aan', 'jeugdspel') ('jeugdspel', '(') ('(', '6-14') ('6-14', 'jaar') ('jaar', ')') (')', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'het') ('het', 'leuk') ('leuk', 'om') ('om', 'na') ('na', 'te') ('te', 'denken') ('denken', 'en') ('en', 'te') ('te', 'discussieren') ('discussieren', 'over') ('over', 'politiek') ('politiek', 'en') ('en', 'maatschappelijk') ('maatschappelijk', 'vraagstukken') ('vraagstukken', '.') ('.', 'Weten') ('Weten', 'wat') ('wat', 'er') ('er', 'speelt') ('speelt', 'in') ('in', 'verschillend') ('verschillend', 'lagen') ('lagen', 'van') ('van', 'de') ('de', 'samenlev') ('samenlev', 'is') ('is', 'daarvoor') ('daarvoor', 'essentieel') ('essentieel', 'en') ('en', 'ik') ('