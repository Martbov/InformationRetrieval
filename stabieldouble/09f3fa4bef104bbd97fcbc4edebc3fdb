an') ('kan', 'bevorderen') ('bevorderen', '.') ('.', 'Misschien') ('Misschien', 'kan') ('kan', 'ik') ('ik', 'dan') ('dan', 'mijn') ('mijn', 'ervaringen') ('ervaringen', 'gebruiken') ('gebruiken', 'om') ('om', 'ander') ('ander', 'mensen') ('mensen', 'te') ('te', 'helpen') ('helpen', '.') ('.', 'Ik') ('Ik', 'denk') ('denk', 'ook') ('ook', 'dat') ('dat', 'het') ('het', 'Rijkstraineeprogramma') ('Rijkstraineeprogramma', 'ook') ('ook', 'mij') ('mij', 'meer') ('meer', 'verbondenheid') ('verbondenheid', 'kan') ('kan', 'geven') ('geven', 'met') ('met', 'Nederland') ('Nederland', '.') ('.', 'Al') ('Al', 'iemand') ('iemand', 'die') ('die', 'niet') ('niet', 'in') ('in', 'Nederland') ('Nederland', 'is') ('is', 'opgegroeid') ('opgegroeid', 'maar') ('maar', 'zich') ('zich', 'wel') ('wel', 'Nederland') ('Nederland', 'noemt') ('noemt', 'wil') ('wil', 'ik') ('ik', 'meer') ('meer', 'te') ('te', 'weten') ('weten', 'krijgen') ('krijgen', 'over') ('over', 'hoe') ('hoe', 'Nederland') ('Nederland', 'nou') ('nou', 'eigenlijk') ('eigenlijk', 'in') ('in', 'elkaar') ('elkaar', 'zit') ('zit', '.') ('.', 'Omdat') ('Omdat', 'ik') ('ik', 'in') ('in', 'verschillend') ('verschillend', 'landen') ('landen', 'heb') ('heb', 'gewoond') ('gewoond', 'ben') ('ben', 'ik') ('ik', 'in') ('in', 'contact') ('contact', 'gekomen') ('gekomen', 'met') ('met', 'verschillend') ('verschillend', 'maatschappijen') ('maatschappijen', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'gemerkt') ('gemerkt', 'dat') ('dat', 'elk') ('elk', 'zijn') ('zijn', 'eigen') ('eigen', 'karakt') ('karakt', 'en') ('en', 'kenmerken') ('kenmerken', 'heeft') ('heeft', '.') ('.', 'Door') ('Door', 'die') ('die', 'te') ('te', 'doorgronden') ('doorgronden', 'kan') ('kan', 'er') ('er', 'een') ('een', 'beter') ('beter', 'samenwerk') ('samenwerk', 'ontstaan') ('ontstaan', 'tussen') ('tussen', 'die') ('die', 'individuen') ('individuen', 'die') ('die', 'de') ('de', 'maatschappij') ('maatschappij', 'telt') ('telt', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'een') 