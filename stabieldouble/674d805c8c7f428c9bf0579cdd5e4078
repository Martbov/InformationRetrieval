in') ('in', 'opdrachten') ('opdrachten', 'en') ('en', 'omgev') ('omgev', 'mij') ('mij', 'erg') ('erg', 'aan') ('aan', '.') ('.', 'Het') ('Het', 'lijkt') ('lijkt', 'mij') ('mij', 'daarom') ('daarom', 'een') ('een', 'uitdag') ('uitdag', 'om') ('om', 'al') ('al', 'traine') ('traine', 'bij') ('bij', 'de') ('de', 'Rijksoverheid') ('Rijksoverheid', 'aan') ('aan', 'de') ('de', 'slag') ('slag', 'te') ('te', 'gaan') ('gaan', '.') ('.', 'Mijn') ('Mijn', 'brede') ('brede', 'interess') ('interess', 'komt') ('komt', 'niet') ('niet', 'alleen') ('alleen', 'tot') ('tot', 'uit') ('uit', 'in') ('in', 'mijn') ('mijn', 'studiekeuz') ('studiekeuz', ',') (',', 'maar') ('maar', 'ook') ('ook', 'in') ('in', 'mijn') ('mijn', 'buitenschools') ('buitenschools', 'activiteiten') ('activiteiten', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'meerder') ('meerder', 'sporten') ('sporten', 'beoefend') ('beoefend', ',') (',', 'en') ('en', 'mij') ('mij', 'de') ('de', 'laatst') ('laatst', 'jaren') ('jaren', 'vooral') ('vooral', 'toegelegd') ('toegelegd', 'op') ('op', 'tenni') ('tenni', '.') ('.', 'Ik') ('Ik', 'acht') ('acht', 'het') ('het', 'niet') ('niet', 'alleen') ('alleen', 'belangrijk') ('belangrijk', 'om') ('om', 'zelf') ('zelf', 'deze') ('deze', 'sporten') ('sporten', 'te') ('te', 'beoefenen') ('beoefenen', ',') (',', 'maar') ('maar', 'ik') ('ik', 'maak') ('maak', 'me') ('me', 'ook') ('ook', 'verdienstelijk') ('verdienstelijk', 'voor') ('voor', 'het') ('het', 'verenigingsleven') ('verenigingsleven', '.') ('.', 'Na') ('Na', 'enkel') ('enkel', 'jaren') ('jaren', 'vooral') ('vooral', 'betrokken') ('betrokken', 'te') ('te', 'zijn') ('zijn', 'geweest') ('geweest', 'bij') ('bij', 'de') ('de', 'organisati') ('organisati', 'van') ('van', 'divers') ('divers', 'jeugdactiviteiten') ('jeugdactiviteiten', ',') (',', 'ben') ('ben', 'ik') ('ik', 'sind') ('sind', 'juli') ('juli', '2008') ('2008', 'bestuurslid') ('bestuurslid', 'van') ('van', 'de') ('de', 'grootst') ('grootst', 'tennisverenig') ('tennisverenig', 'van')