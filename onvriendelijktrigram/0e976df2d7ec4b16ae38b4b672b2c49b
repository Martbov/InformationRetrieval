elf') ('voor', 'mijzelf', 'beter') ('mijzelf', 'beter', 'te') ('beter', 'te', 'kunnen') ('te', 'kunnen', 'bepalen') ('kunnen', 'bepalen', 'wat') ('bepalen', 'wat', 'ik') ('wat', 'ik', 'nu') ('ik', 'nu', 'echt') ('nu', 'echt', 'later') ('echt', 'later', 'wil') ('later', 'wil', 'en') ('wil', 'en', 'wat') ('en', 'wat', 'mijn') ('wat', 'mijn', 'mogelijkheden') ('mijn', 'mogelijkheden', 'zijn') ('mogelijkheden', 'zijn', '.') ('zijn', '.', 'Mijn') ('.', 'Mijn', 'maatschappelijk') ('Mijn', 'maatschappelijk', 'betrokkenheid') ('maatschappelijk', 'betrokkenheid', 'richt') ('betrokkenheid', 'richt', 'zich') ('richt', 'zich', 'op') ('zich', 'op', 'het') ('op', 'het', 'willen') ('het', 'willen', 'doen') ('willen', 'doen', 'van') ('doen', 'van', 'onderzoek/werk') ('van', 'onderzoek/werk', 'waar') ('onderzoek/werk', 'waar', 'men') ('waar', 'men', 'in') ('men', 'in', 'de') ('in', 'de', 'praktijk') ('de', 'praktijk', 'iet') ('praktijk', 'iet', 'aan') ('iet', 'aan', 'heeft') ('aan', 'heeft', '.') ('heeft', '.', 'En') ('.', 'En', 'daarme') ('En', 'daarme', 'mijn') ('daarme', 'mijn', 'bijdrag') ('mijn', 'bijdrag', 'te') ('bijdrag', 'te', 'kunnen') ('te', 'kunnen', 'leveren') ('kunnen', 'leveren', 'aan') ('leveren', 'aan', 'een') ('aan', 'een', 'duurzam') ('een', 'duurzam', 'milieu') ('duurzam', 'milieu', '.') ('milieu', '.', 'Ik') ('.', 'Ik', 'wil') ('Ik', 'wil', 'iet') ('wil', 'iet', 'kunnen') ('iet', 'kunnen', 'betekenen') ('kunnen', 'betekenen', 'voor') ('betekenen', 'voor', 'de') ('voor', 'de', 'maatschappij') ('de', 'maatschappij', '(') ('maatschappij', '(', 'men') ('(', 'men', ',') ('men', ',', 'dier') (',', 'dier', 'en') ('dier', 'en', 'milieu') ('en', 'milieu', ')') ('milieu', ')', 'en') (')', 'en', 'daarbij') ('en', 'daarbij', 'papier') ('daarbij', 'papier', 'in') ('papier', 'in', 'praktijk') ('in', 'praktijk', 'omzetten') ('praktijk', 'omzetten', '.')