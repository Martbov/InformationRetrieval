i') ('commissi', ',') (',', 'waarond') ('waarond', 'de') ('de', 'onderwijscommissi') ('onderwijscommissi', '.') ('.', 'Deze') ('Deze', 'commissi') ('commissi', 'is') ('is', 'een') ('een', 'advieserend') ('advieserend', 'orgaan') ('orgaan', 'naar') ('naar', 'het') ('het', 'bestuur') ('bestuur', 'van') ('van', 'de') ('de', 'opleid') ('opleid', '.') ('.', 'Ze') ('Ze', 'houden') ('houden', 'zaken') ('zaken', 'al') ('al', 'onderwijskwaliteit') ('onderwijskwaliteit', ',') (',', 'tentamin') ('tentamin', ',') (',', 'verroost') ('verroost', ',') (',', 'curricula') ('curricula', 'e.d') ('e.d', '.') ('.', 'in') ('in', 'de') ('de', 'gaten') ('gaten', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'altijd') ('altijd', 'met') ('met', 'veel') ('veel', 'plezier') ('plezier', 'in') ('in', 'deze') ('deze', 'commissi') ('commissi', 'zit') ('zit', 'genomen') ('genomen', 'om') ('om', 'op') ('op', 'deze') ('deze', 'manier') ('manier', 'mijn') ('mijn', 'steentj') ('steentj', 'bij') ('bij', 'te') ('te', 'dragen') ('dragen', 'aan') ('aan', 'de') ('de', 'verbet') ('verbet', 'van') ('van', 'het') ('het', 'onderwij') ('onderwij', '.') ('Het', 'technisch', 'en') ('technisch', 'en', 'onderzoeksgericht') ('en', 'onderzoeksgericht', 'heeft') ('onderzoeksgericht', 'heeft', 'mij') ('heeft', 'mij', 'altijd') ('mij', 'altijd', 'erg') ('altijd', 'erg', 'aangesproken') ('erg', 'aangesproken', 'in') ('aangesproken', 'in', 'mijn') ('in', 'mijn', 'studi') ('mijn', 'studi', ',') ('studi', ',', 'maar') (',', 'maar', 'wat') ('maar', 'wat', 'mij') ('wat', 'mij', 'vooral') ('mij', 'vooral', 'aansprak') ('vooral', 'aansprak', 'wa') ('aansprak', 'wa', 'de') ('wa', 'de', 'veelzijdigheid') ('de', 'veelzijdigheid', 'van') ('veelzijdigheid', 'van', 'BioMedisch') ('van', 'BioMedisch', 'Technologi') ('BioMedisch', 'Technologi', '(') ('Technologi', '(', 'BMT') ('(', 'BMT', ')') ('BMT', ')', '.') (')', '.', 'Bij') ('.', 'Bij', 'BMT') ('Bij', 'BMT', 'kom') ('BMT', 'kom', 'je') ('kom', 'je', 'in') ('je', 'in', 'aanrak') ('in', 'aa