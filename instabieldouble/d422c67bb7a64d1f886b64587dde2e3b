et') ('het', 'volgen') ('volgen', 'van') ('van', 'actualiteiten') ('actualiteiten', '.') ('.', 'Daarnaast') ('Daarnaast', 'is') ('is', 'typerend') ('typerend', 'voor') ('voor', 'mij') ('mij', 'dat') ('dat', 'ik') ('ik', 'ook') ('ook', 'ga') ('ga', 'stemmen') ('stemmen', 'al') ('al', 'ik') ('ik', 'met') ('met', '39') ('39', 'graden') ('graden', 'koort') ('koort', 'en') ('en', 'een') ('een', 'longontstek') ('longontstek', 'op') ('op', 'bed') ('bed', 'lig') ('lig', '.') ('.', 'Mijn') ('Mijn', 'stem') ('stem', 'en') ('en', 'de') ('de', 'democrati') ('democrati', 'zijn') ('zijn', 'belangrijk') ('belangrijk', '.') ('.', 'Ik') ('Ik', 'motiv') ('motiv', 'mijn') ('mijn', 'omgev') ('omgev', 'om') ('om', 'zelf') ('zelf', 'te') ('te', 'denken') ('denken', 'en') ('en', 'vervolgen') ('vervolgen', 'een') ('een', 'stem') ('stem', 'uitbrengen') ('uitbrengen', 'waar') ('waar', 'ze') ('ze', 'achter') ('achter', 'staan') ('staan', '.') ('.', 'Al') ('Al', 'betrokken') ('betrokken', 'burger') ('burger', 'kom') ('kom', 'ik') ('ik', 'graag') ('graag', 'in') ('in', 'contact') ('contact', 'met') ('met', 'verschillend') ('verschillend', 'mensen') ('mensen', 'en') ('en', 'ben') ('ben', 'ik') ('ik', 'geïnteresseerd') ('geïnteresseerd', 'in') ('in', 'de') ('de', 'mene') ('mene', 'van') ('van', 'een') ('een', 'ieder') ('ieder', '.') ('.', 'Mijn') ('Mijn', 'brede') ('brede', 'interess') ('interess', 'uiten') ('uiten', 'zich') ('zich', 'verder') ('verder', 'in') ('in', 'de') ('de', 'divers') ('divers', 'sporten') ('sporten', 'die') ('die', 'ik') ('ik', 'beoefen') ('beoefen', 'en') ('en', 'beoefend') ('beoefend', 'heb') ('heb', ',') (',', 'mijn') ('mijn', 'hobby¿') ('hobby¿', 'lezen') ('lezen', 'en') ('en', 'social') ('social', 'activiteiten') ('activiteiten', 'organiseren') ('organiseren', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'opgegroeid') ('opgegroeid', 'in') ('in', 'een') ('een', 'kerkelijk') ('kerkelijk', 'stad') ('stad', 'op') ('op', 'de') ('de', 'Veluw') ('Veluw', 'en') ('en', 'heb') ('heb',