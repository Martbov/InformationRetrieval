') ('kinderen', 'met') ('met', 'overgewicht') ('overgewicht', '.') ('.', 'Maatschappelijk') ('Maatschappelijk', 'zaken') ('zaken', 'hebben') ('hebben', 'mij') ('mij', 'altijd') ('altijd', 'geïnteresseerd') ('geïnteresseerd', ',') (',', 'gerelateerd') ('gerelateerd', 'aan') ('aan', 'de') ('de', 'problemen') ('problemen', 'die') ('die', 'ik') ('ik', 'tijden') ('tijden', 'mijn') ('mijn', '(') ('(', 'vrijwillig') ('vrijwillig', ')') (')', 'werk') ('werk', 'en') ('en', 'stage') ('stage', 'tegenkwam') ('tegenkwam', ':') (':', 'welk') ('welk', 'rol') ('rol', 'moet') ('moet', 'de') ('de', 'overheid') ('overheid', 'spelen') ('spelen', 'in') ('in', 'de') ('de', 'strijd') ('strijd', 'tegen') ('tegen', 'overgewicht') ('overgewicht', '?') ('?', 'Op') ('Op', 'welk') ('welk', 'manier') ('manier', 'kan') ('kan', 'het') ('het', 'vak') ('vak', 'van') ('van', 'docent') ('docent', 'aantrekkelijk') ('aantrekkelijk', 'worden') ('worden', 'gemaakt') ('gemaakt', '?') ('?', 'Op') ('Op', 'dit') ('dit', 'moment') ('moment', 'rei') ('rei', 'ik') ('ik', 'door') ('door', 'Zuid-Amerika') ('Zuid-Amerika', 'en') ('en', 'volgend') ('volgend', 'week') ('week', 'begint') ('begint', 'mijn') ('mijn', 'vrijwilligerswerk') ('vrijwilligerswerk', 'in') ('in', 'een') ('een', 'kindertehui') ('kindertehui', 'in') ('in', 'Sucr') ('Sucr', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'erg') ('erg', 'blij') ('blij', 'dat') ('dat', 'ik') ('ik', 'op') ('op', 'die') ('die', 'manier') ('manier', 'iet') ('iet', 'terug') ('terug', 'kan') ('kan', 'doen') ('doen', 'voor') ('voor', 'dit') ('dit', 'mooie') ('mooie', ',') (',', 'maar') ('maar', 'bizarr') ('bizarr', 'contin') ('contin', '.')