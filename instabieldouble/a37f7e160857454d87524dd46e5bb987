') ('daar', 'ik') ('ik', 'resultaatgericht') ('resultaatgericht', ',') (',', 'optimistisch') ('optimistisch', 'en') ('en', 'ondernemend') ('ondernemend', 'ben') ('ben', '.') ('.', 'Ook') ('Ook', 'heeft') ('heeft', 'international') ('international', 'ervar') ('ervar', 'mijn') ('mijn', 'adaptiviteit') ('adaptiviteit', 'en') ('en', 'flexibiliteit') ('flexibiliteit', 'vergroot') ('vergroot', 'blijf') ('blijf', 'ik') ('ik', 'rustig') ('rustig', 'in') ('in', 'stressvol') ('stressvol', 'situati') ('situati', '.') ('.', 'Verder') ('Verder', 'schep') ('schep', 'ik') ('ik', 'genoegen') ('genoegen', 'in') ('in', 'het') ('het', 'samenbrengen') ('samenbrengen', 'van') ('van', 'mensen') ('mensen', 'en') ('en', 'standpunten') ('standpunten', '.') ('.', 'Tot') ('Tot', 'slot') ('slot', 'kan') ('kan', 'ik') ('ik', 'go') ('go', 'reflecteren') ('reflecteren', '(') ('(', 'op') ('op', 'mijzelf') ('mijzelf', 'en') ('en', 'op') ('op', 'anderen') ('anderen', ')') (')', 'en') ('en', 'kritiek') ('kritiek', 'incasseren') ('incasseren', '.') ('.', 'Maatschappelijk') ('Maatschappelijk', 'betrokken') ('betrokken', 'is') ('is', 'mijn') ('mijn', 'inzien') ('inzien', 'diegen') ('diegen', 'die') ('die', 'zich') ('zich', 'deel') ('deel', 'voelt') ('voelt', 'en') ('en', 'zich') ('zich', 'het') ('het', 'wel') ('wel', 'en') ('en', 'wee') ('wee', 'aantrekt') ('aantrekt', 'van') ('van', 'een') ('een', 'maatschappij') ('maatschappij', '.') ('.', 'Hierbij') ('Hierbij', 'wil') ('wil', 'men') ('men', 'zich') ('zich', 'inzetten') ('inzetten', 'om') ('om', 'het') ('het', '¨wee¨') ('¨wee¨', 'weg') ('weg', 'te') ('te', 'nemen') ('nemen', '.') ('.', 'Het') ('Het', 'is') ('is', 'interess') ('interess', 'voor') ('voor', 'dien') ('dien', 'leefwereld') ('leefwereld', 'en') ('en', 'voor') ('voor', 'het') ('het', 'welzijn') ('welzijn', 'van') ('van', 'de') ('de', 'gehel') ('gehel', 'maatschappij') ('maatschappij', '.') ('.', 'Bij') ('Bij', 'mij') ('mij', 'uit') ('uit', 'dit') ('dit', 'zich') ('zich', 'in') ('in', 'het') 