Ook') ('Ook', 'ben') ('ben', 'ik') ('ik', '(') ('(', 'onbetaald') ('onbetaald', ')') (')', 'redactielid') ('redactielid', 'bij') ('bij', 'een') ('een', 'juridisch') ('juridisch', 'tijdschrift') ('tijdschrift', 'dat') ('dat', 'zich') ('zich', 'op') ('op', 'juridisch') ('juridisch', 'en') ('en', 'maatschappelijk') ('maatschappelijk', 'onderwerpen') ('onderwerpen', 'richt') ('richt', '.') ('.', 'Ook') ('Ook', 'doneer') ('doneer', 'ik') ('ik', 'geld') ('geld', 'aan') ('aan', 'goed') ('goed', 'doelen') ('doelen', 'en') ('en', 'heb') ('heb', 'ik') ('ik', 'tijden') ('tijden', 'mijn') ('mijn', 'basis-') ('basis-', 'en') ('en', 'middelbar') ('middelbar', 'school') ('school', 'period') ('period', 'geld') ('geld', 'ingezameld') ('ingezameld', 'voor') ('voor', 'goed') ('goed', 'doelen') ('doelen', 'door') ('door', 'middel') ('middel', 'van') ('van', 'sponsorlopen') ('sponsorlopen', 'en') ('en', 'collecteren') ('collecteren', '.') ('.', 'Teven') ('Teven', 'blijkt') ('blijkt', 'mijn') ('mijn', 'politieke/maatschappelijk') ('politieke/maatschappelijk', 'interess') ('interess', 'uit') ('uit', 'het') ('het', 'feit') ('feit', 'dat') ('dat', 'ik') ('ik', 'afgelopen') ('afgelopen', 'gemeenteraadsverkiezingen') ('gemeenteraadsverkiezingen', 'meegeholpen') ('meegeholpen', 'heb') ('heb', 'met') ('met', 'het') ('het', 'tellen') ('tellen', 'van') ('van', 'de') ('de', 'stemmen') ('stemmen', '.') ('.', 'Ook') ('Ook', 'blijkt') ('blijkt', 'deze') ('deze', 'interess') ('interess', 'uit') ('uit', 'het') ('het', 'onderwerp') ('onderwerp', 'van') ('van', 'mijn') ('mijn', 'scripti') ('scripti', ':') (':', "'het") ("'het", 'aanbevelings-') ('aanbevelings-', 'en') ('en', 'vetorecht') ('vetorecht', 'van') ('van', 'de') ('de', 'door') ('door', 'de') ('de', 'Staat') ('Staat', 'aanbevolen') ('aanbevolen', 'commissarissen') ('commissarissen', 'bij') ('bij', 'de') ('de', 'kapitaalversterk') ('kapitaalversterk', 'bij') ('bij', 'ING') ('ING', ',') (',', 'Aegon') ('Aegon', 'en') ('en', 'SN') ('SN', 'Reaal') (