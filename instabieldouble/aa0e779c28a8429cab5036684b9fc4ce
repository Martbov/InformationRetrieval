a') ('wa', 'ik') ('ik', 'teven') ('teven', 'commissielid') ('commissielid', 'intern') ('intern', 'schole') ('schole', '.') ('.', 'Ik') ('Ik', 'heb') ('heb', 'bij') ('bij', 'de') ('de', 'NPCF') ('NPCF', 'gewerkt') ('gewerkt', 'die') ('die', 'zich') ('zich', 'inzet') ('inzet', 'voor') ('voor', 'de') ('de', 'belangen') ('belangen', 'van') ('van', 'patiënten') ('patiënten', 'in') ('in', 'de') ('de', 'zorg') ('zorg', '.') ('.', 'Aansluitend') ('Aansluitend', 'wild') ('wild', 'ik') ('ik', 'iet') ('iet', 'meer') ('meer', 'betekenen') ('betekenen', 'voor') ('voor', 'deze') ('deze', 'groep') ('groep', 'en') ('en', 'heb') ('heb', 'besloten') ('besloten', 'om') ('om', 'al') ('al', 'vrijwillig') ('vrijwillig', 'bij') ('bij', 'de') ('de', 'Rode') ('Rode', 'Krui') ('Krui', 'in') ('in', 'te') ('te', 'schrijven') ('schrijven', '.') ('.', 'Naast') ('Naast', 'het') ('het', 'collecteren') ('collecteren', 'een') ('een', 'keer') ('keer', 'in') ('in', 'het') ('het', 'jaar') ('jaar', ',') (',', 'ben') ('ben', 'ik') ('ik', 'aangesloten') ('aangesloten', 'bij') ('bij', 'de') ('de', 'afdel') ('afdel', 'sociaal') ('sociaal', 'huisbezoek') ('huisbezoek', 'en') ('en', 'bij') ('bij', 'de') ('de', 'afdel') ('afdel', 'opvang') ('opvang', 'en') ('en', 'verzorg') ('verzorg', '.') ('.', 'In') ('In', '2004') ('2004', 'ben') ('ben', 'ik') ('ik', 'stembureaulid') ('stembureaulid', 'bij') ('bij', 'het') ('het', 'bureau') ('bureau', 'verkiezingen') ('verkiezingen', 'in') ('in', 'Utrecht') ('Utrecht', '.')