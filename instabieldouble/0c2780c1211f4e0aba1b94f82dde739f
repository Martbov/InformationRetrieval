raag') ('graag', 'verder') ('verder', 'ontwikkelen') ('ontwikkelen', 'door') ('door', 'met') ('met', 'verschillend') ('verschillend', 'mensen') ('mensen', ',') (',', 'departementen') ('departementen', 'en') ('en', 'projecten') ('projecten', 'te') ('te', 'werken') ('werken', '.') ('.', 'Ten') ('Ten', 'derd') ('derd', 'wil') ('wil', 'ik') ('ik', 'altijd') ('altijd', 'nieuw') ('nieuw', 'dingen') ('dingen', 'leren') ('leren', 'en') ('en', 'nieuw') ('nieuw', 'uitdagingen') ('uitdagingen', 'aangaan') ('aangaan', '.') ('.', 'Kortom') ('Kortom', ',') (',', 'dit') ('dit', 'programma') ('programma', 'biedt') ('biedt', 'mij') ('mij', 'dit') ('dit', ',') (',', 'door') ('door', 'een') ('een', 'combinati') ('combinati', 'van') ('van', 'begeleid') ('begeleid', 'en') ('en', 'de') ('de', 'mogelijkheid') ('mogelijkheid', 'om') ('om', '¿over') ('¿over', 'de') ('de', 'schotten¿') ('schotten¿', 'heen') ('heen', 'te') ('te', 'kijken') ('kijken', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'een') ('een', 'actief') ('actief', 'en') ('en', 'sociaal') ('sociaal', 'persoon') ('persoon', 'en') ('en', 'ben') ('ben', 'altijd') ('altijd', 'betrokken') ('betrokken', '(') ('(', 'geweest') ('geweest', ')') (')', 'bij') ('bij', 'wat') ('wat', 'zich') ('zich', 'in') ('in', 'mijn') ('mijn', 'omgev') ('omgev', 'afspeelt') ('afspeelt', '.') ('.', 'Aan') ('Aan', 'de') ('de', 'ene') ('ene', 'kant') ('kant', 'is') ('is', 'deze') ('deze', 'betrokkenheid') ('betrokkenheid', 'passief') ('passief', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'het') ('het', 'belangrijk') ('belangrijk', 'om') ('om', 'een') ('een', 'zo') ('zo', 'geïnformeerd') ('geïnformeerd', 'mogelijk') ('mogelijk', 'mene') ('mene', 'te') ('te', 'vormen') ('vormen', ',') (',', 'daarom') ('daarom', 'lee') ('lee', 'ik') ('ik', 'graag') ('graag', 'veel') ('veel', 'boeken') ('boeken', 'en') ('en', 'volg') ('volg', 'ik') ('ik', 'het') ('het', 'nieuw') ('nieuw', 'via') ('via', 'kranten') ('kranten', ',') (',', 'internet') ('internet', 'en') ('en', 'tv') ('tv'