aagstukken', 'zoal') ('zoal', 'multiculturalism') ('multiculturalism', 'en') ('en', 'integrati') ('integrati', '.') ('.', 'Hoewel') ('Hoewel', 'het') ('het', 'percentag') ('percentag', 'van') ('van', "'nieuw") ("'nieuw", 'Nederland') ('Nederland', "'") ("'", 'die') ('die', 'studeren') ('studeren', ',') (',', 'werken') ('werken', 'of') ('of', 'politiek') ('politiek', 'actief') ('actief', 'zijn') ('zijn', 'toeneemt') ('toeneemt', ',') (',', 'is') ('is', 'het') ('het', 'nog') ('nog', 'niet') ('niet', 'voldend') ('voldend', '.') ('.', 'Vanuit') ('Vanuit', 'mijn') ('mijn', 'perspectief') ('perspectief', ',') (',', 'om') ('om', 'een') ('een', 'wederkerig') ('wederkerig', 'integrati') ('integrati', 'en') ('en', 'communicati') ('communicati', 'te') ('te', 'bevorderen') ('bevorderen', 'is') ('is', 'het') ('het', 'essentieel') ('essentieel', 'dat') ('dat', 'de') ('de', "'nieuw") ("'nieuw", "'") ("'", 'en') ('en', 'de') ('de', "'oud") ("'oud", "'") ("'", 'Nederland') ('Nederland', 'samenwerken') ('samenwerken', '.') ('.', 'Hoewel') ('Hoewel', 'het') ('het', 'moeilijk') ('moeilijk', 'kan') ('kan', 'zijn') ('zijn', 'om') ('om', 'aan') ('aan', 'te') ('te', 'tonen') ('tonen', ',') (',', 'ben') ('ben', 'ik') ('ik', 'persoonlijk') ('persoonlijk', 'geintereseerd') ('geintereseerd', 'in') ('in', 'de') ('de', 'volgend') ('volgend', 'vragen') ('vragen', ':') (':', 'wat') ('wat', 'voor') ('voor', 'impact') ('impact', 'maakt') ('maakt', 'de') ('de', 'label') ('label', 'van') ('van', 'buitenland') ('buitenland', 'op') ('op', 'het') ('het', 'integratieprocess') ('integratieprocess', ',') (',', 'in') ('in', 'positiev') ('positiev', 'en') ('en', 'negatiev') ('negatiev', 'zin') ('zin', '?') ('?', 'Wat') ('Wat', 'betek') ('betek', 'dat') ('dat', 'voor') ('voor', 'de') ('de', 'nativ') ('nativ', 'en') ('en', 'non-n') ('non-n', 'Nederland') ('Nederland', 'tegelijk') ('tegelijk', '?') ('?', 'Betek') ('Betek', 'veranderen') ('veranderen', 'van') ('van', 'label') ('label', 'dat') ('dat', 'ook') ('ook', 'percepti') ('percepti', 'veranderen') ('veranderen', '?')