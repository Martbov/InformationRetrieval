ast') ('Daarnaast', 'volg') ('volg', 'ik') ('ik', 'het') ('het', 'nieuw') ('nieuw', 'op') ('op', 'de') ('de', 'voet') ('voet', 'en') ('en', 'ga') ('ga', 'ik') ('ik', 'daarov') ('daarov', 'graag') ('graag', 'in') ('in', 'discussi') ('discussi', 'met') ('met', 'mensen') ('mensen', 'in') ('in', 'mijn') ('mijn', 'omgev') ('omgev', '.') ('.', 'Hierme') ('Hierme', 'probeer') ('probeer', 'ik') ('ik', 'ook') ('ook', 'ander') ('ander', 'mensen') ('mensen', 'bewust') ('bewust', 'te') ('te', 'maken') ('maken', 'van') ('van', 'de') ('de', 'dingen') ('dingen', 'die') ('die', 'om') ('om', 'on') ('on', 'heen') ('heen', 'gebeuren') ('gebeuren', '.') ('.', 'Mijn') ('Mijn', 'doel') ('doel', 'hiervan') ('hiervan', 'is') ('is', 'mensen') ('mensen', 'te') ('te', 'stimuleren') ('stimuleren', 'zich') ('zich', 'ook') ('ook', 'in') ('in', 'te') ('te', 'zetten') ('zetten', 'voor') ('voor', 'de') ('de', 'maatschappij') ('maatschappij', 'en') ('en', 'du') ('du', 'de') ('de', 'medemen') ('medemen', '.')