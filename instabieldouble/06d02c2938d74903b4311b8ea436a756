') (',', 'dat') ('dat', 'ik') ('ik', 'in') ('in', 'samenwerk') ('samenwerk', 'met') ('met', 'Rijkswaterstaat') ('Rijkswaterstaat', 'uitvoer') ('uitvoer', '.') ('.', 'De') ('De', 'samenwerk') ('samenwerk', 'met') ('met', 'Rijkswaterstaat') ('Rijkswaterstaat', 'ervaar') ('ervaar', 'ik') ('ik', 'al') ('al', 'zeer') ('zeer', 'positief') ('positief', '.') ('.', 'Ook') ('Ook', 'de') ('de', 'interacti') ('interacti', 'met') ('met', 'provinci') ('provinci', ',') (',', 'gemeenten') ('gemeenten', 'en') ('en', 'ander') ('ander', 'belanghebbenden') ('belanghebbenden', 'spreekt') ('spreekt', 'mij') ('mij', 'aan') ('aan', '.') ('.', 'Het') ('Het', 'lijkt') ('lijkt', 'mij') ('mij', 'interess') ('interess', 'om') ('om', 'al') ('al', 'medewerk') ('medewerk', 'bij') ('bij', 'de') ('de', 'Rijksoverheid') ('Rijksoverheid', 'verschillend') ('verschillend', 'actoren') ('actoren', 'samen') ('samen', 'te') ('te', 'brengen') ('brengen', '.') ('.', 'Mijn') ('Mijn', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'spreekt') ('spreekt', 'uit') ('uit', 'mijn') ('mijn', 'interess') ('interess', 'voor') ('voor', 'de') ('de', 'onderwerpen') ('onderwerpen', 'RO') ('RO', 'en') ('en', 'water') ('water', 'manag') ('manag', '.') ('.', 'In') ('In', 'de') ('de', 'RO') ('RO', 'zorg') ('zorg', 'je') ('je', 'ervoor') ('ervoor', 'dat') ('dat', 'de') ('de', 'inricht') ('inricht', 'van') ('van', 'een') ('een', 'gebi') ('gebi', 'aansluit') ('aansluit', 'bij') ('bij', 'de') ('de', 'wensen') ('wensen', 'van') ('van', 'de') ('de', 'burger') ('burger', '.') ('.', 'Het') ('Het', 'hoofddoel') ('hoofddoel', 'van') ('van', 'water') ('water', 'manag') ('manag', 'is') ('is', 'om') ('om', 'de') ('de', 'veiligheid') ('veiligheid', 'te') ('te', 'garanderen') ('garanderen', '.') ('.', 'Hierbij') ('Hierbij', 'is') ('is', 'het') ('het', 'belangrijk') ('belangrijk', 'om') ('om', 'draagvlak') ('draagvlak', 'en') ('en', 'bewustzijn') ('bewustzijn', 'te') ('te', 'creëren') ('creëren', '.') ('.', 'Vandaar'