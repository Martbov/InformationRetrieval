) ('het', 'Westen') ('Westen', 'en') ('en', 'de') ('de', 'Arabisch') ('Arabisch', 'wereld') ('wereld', '.') ('.', 'Om') ('Om', 'de') ('de', 'relati') ('relati', 'beter') ('beter', 'te') ('te', 'begrijpen') ('begrijpen', 'studeer') ('studeer', 'ik') ('ik', 'naast') ('naast', 'rechten') ('rechten', 'ook') ('ook', 'Arabisch') ('Arabisch', 'en') ('en', 'heb') ('heb', 'ik') ('ik', 'me') ('me', 'beziggehouden') ('beziggehouden', 'met') ('met', 'religiestudi') ('religiestudi', '.') ('.', 'Op') ('Op', 'all') ('all', 'terreinen') ('terreinen', 'is') ('is', 'mijn') ('mijn', 'uitgangspunt') ('uitgangspunt', 'dat') ('dat', 'het') ('het', 'verspreiden') ('verspreiden', 'van') ('van', 'kenni') ('kenni', 'de') ('de', 'best') ('best', 'manier') ('manier', 'is') ('is', 'om') ('om', 'een') ('een', 'samenlev') ('samenlev', 'te') ('te', 'verheffen') ('verheffen', '.')