en') ('ben', 'namelijk') ('namelijk', 'geïnteresseerd') ('geïnteresseerd', 'in') ('in', 'de') ('de', 'rol') ('rol', 'die') ('die', 'communicati') ('communicati', 'en') ('en', 'vertrouwen') ('vertrouwen', 'kan') ('kan', 'hebben') ('hebben', 'om') ('om', 'de') ('de', 'burger') ('burger', 'mee') ('mee', 'te') ('te', 'laten') ('laten', 'doen') ('doen', 'aan') ('aan', 'de') ('de', 'besluitvorm') ('besluitvorm', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'het') ('het', 'daarnaast') ('daarnaast', 'belangrijk') ('belangrijk', 'dat') ('dat', 'mijn') ('mijn', 'werk') ('werk', 'bruikbaar') ('bruikbaar', 'is') ('is', '.') ('.', 'Gewon') ('Gewon', 'mensen') ('mensen', 'moeten') ('moeten', 'er') ('er', 'iet') ('iet', 'mee') ('mee', 'kunnen') ('kunnen', '.') ('.', 'Ik') ('Ik', 'wil') ('wil', 'niet') ('niet', 'van') ('van', 'achter') ('achter', 'mijn') ('mijn', 'bureau') ('bureau', 'dingen') ('dingen', 'bedenken') ('bedenken', 'die') ('die', 'in') ('in', 'de') ('de', 'praktijk') ('praktijk', 'niet') ('niet', 'werken') ('werken', '.') ('.', 'Mijn') ('Mijn', 'stage') ('stage', 'doe') ('doe', 'ik') ('ik', 'dan') ('dan', 'ook') ('ook', 'bij') ('bij', 'het') ('het', 'Nirov') ('Nirov', '.') ('.', 'Dit') ('Dit', 'is') ('is', 'en') ('en', 'netwerkorganisati') ('netwerkorganisati', 'die') ('die', 'op') ('op', 'het') ('het', 'gebi') ('gebi', 'van') ('van', 'ruimtelijk') ('ruimtelijk', 'orden') ('orden', 'de') ('de', 'verschillend') ('verschillend', 'praktijkvelden') ('praktijkvelden', 'bij') ('bij', 'elkaar') ('elkaar', 'brengt') ('brengt', '.')