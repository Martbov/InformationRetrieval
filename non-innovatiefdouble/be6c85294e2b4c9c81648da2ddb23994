eb') ('heb', 'ik') ('ik', 'dat') ('dat', 'gedaan') ('gedaan', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'begonnen') ('begonnen', 'met') ('met', 'trainen') ('trainen', 'geven') ('geven', 'op') ('op', 'allerlei') ('allerlei', 'niveau') ('niveau', ',') (',', 'daarnaast') ('daarnaast', 'heb') ('heb', 'ik') ('ik', 'veel') ('veel', 'volleybalkampen') ('volleybalkampen', 'georganiseerd') ('georganiseerd', ',') (',', 'zowel') ('zowel', 'voor') ('voor', 'de') ('de', 'allerkleinsten') ('allerkleinsten', 'al') ('al', 'voor') ('voor', 'de') ('de', 'jeugd') ('jeugd', '.') ('.', 'Ook') ('Ook', 'heb') ('heb', 'ik') ('ik', 'mijn') ('mijn', 'bijdrag') ('bijdrag', 'kunnen') ('kunnen', 'leveren') ('leveren', 'aan') ('aan', 'verschillend') ('verschillend', 'feesten') ('feesten', 'die') ('die', 'georganiseerd') ('georganiseerd', 'werden') ('werden', ',') (',', 'het') ('het', 'draaiend') ('draaiend', 'houden') ('houden', 'van') ('van', 'de') ('de', 'bar') ('bar', 'en') ('en', 'de') ('de', 'kascommissi') ('kascommissi', '.') ('.', 'Naast') ('Naast', 'de') ('de', 'vrijwillig') ('vrijwillig', 'zijn') ('zijn', 'voor') ('voor', 'een') ('een', 'sportverenig') ('sportverenig', 'sponsor') ('sponsor', 'zeer') ('zeer', 'belangrijk') ('belangrijk', '.') ('.', 'Vooral') ('Vooral', 'tijden') ('tijden', 'de') ('de', 'period') ('period', 'dat') ('dat', 'ik') ('ik', 'topsport') ('topsport', 'bedreef') ('bedreef', 'hebben') ('hebben', 'wij') ('wij', 'hier') ('hier', 'al') ('al', 'team') ('team', 'veel') ('veel', 'mee') ('mee', 'te') ('te', 'maken') ('maken', 'gehad') ('gehad', '.') ('.', 'Al') ('Al', 'team') ('team', 'zijnd') ('zijnd', 'hebben') ('hebben', 'wij') ('wij', 'geprobeerd') ('geprobeerd', 'hieraan') ('hieraan', 'mee') ('mee', 'te') ('te', 'helpen') ('helpen', 'door') ('door', 'bij') ('bij', 'bedrijven') ('bedrijven', 'lang') ('lang', 'te') ('te', 'gaan') ('gaan', 'en') ('en', 'sponsoravonden') ('sponsoravonden', 'te') ('te', 'organiseren') ('organiseren', '.')