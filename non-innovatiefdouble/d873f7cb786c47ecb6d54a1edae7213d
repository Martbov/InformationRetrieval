') ('met', 'het') ('het', 'positief') ('positief', 'beinvloeden') ('beinvloeden', 'van') ('van', 'de') ('de', 'verkoop') ('verkoop', 'middel') ('middel', 'het') ('het', 'motiveren') ('motiveren', 'van') ('van', 'groepen') ('groepen', 'telemarkt') ('telemarkt', '.') ('.', 'Teven') ('Teven', 'heb') ('heb', 'ik') ('ik', ',') (',', 'middel') ('middel', 'mijn') ('mijn', 'enthousiasm') ('enthousiasm', 'voor') ('voor', 'goed') ('goed', 'doelen') ('doelen', ',') (',', 'mijn') ('mijn', 'mene') ('mene', 'laten') ('laten', 'vallen') ('vallen', 'aan') ('aan', 'mijn') ('mijn', 'werkgev') ('werkgev', 'over') ('over', 'een') ('een', 'donati') ('donati', 'aan') ('aan', 'goed') ('goed', 'doelen') ('doelen', '.') ('.', 'Hier') ('Hier', 'is') ('is', 'teven') ('teven', 'wat') ('wat', 'meegedaan') ('meegedaan', 'tijden') ('tijden', 'de') ('de', 'kerst') ('kerst', '.') ('.', 'Daarnaast') ('Daarnaast', 'heb') ('heb', 'ik') ('ik', 'ruim') ('ruim', 'twee') ('twee', 'jaar') ('jaar', 'het') ('het', 'certificaat') ('certificaat', 'bedrijfshulpverlen') ('bedrijfshulpverlen', 'in') ('in', 'bezit') ('bezit', 'en') ('en', 'ben') ('ben', 'ik') ('ik', 'lid') ('lid', 'van') ('van', 'Novib') ('Novib', 'en') ('en', 'volg') ('volg', 'ik') ('ik', 'elk') ('elk', 'dag') ('dag', 'het') ('het', 'nieuw') ('nieuw', '.')