i') ('motivati', 'in') ('in', 'al') ('al', 'Rijkstraine') ('Rijkstraine', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'ondernemend') ('ondernemend', ',') (',', 'enthousiast') ('enthousiast', ',') (',', 'flexibel') ('flexibel', 'en') ('en', 'resultaatgericht') ('resultaatgericht', '.') ('.', 'Daarnaast') ('Daarnaast', 'kan') ('kan', 'ik') ('ik', 'zowel') ('zowel', 'in') ('in', 'team') ('team', 'al') ('al', 'zelfstandig') ('zelfstandig', 'opereren') ('opereren', '.') ('.', 'Op') ('Op', 'dit') ('dit', 'moment') ('moment', 'mi') ('mi', 'ik') ('ik', 'de') ('de', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'in') ('in', 'mijn') ('mijn', 'functi') ('functi', '.') ('.', 'Nadat') ('Nadat', 'ik') ('ik', 'gestopt') ('gestopt', 'ben') ('ben', 'al') ('al', 'psycholoog') ('psycholoog', ',') (',', 'ben') ('ben', 'ik') ('ik', 'direct') ('direct', 'gestart') ('gestart', 'met') ('met', 'vrijwilligerswerk') ('vrijwilligerswerk', 'om') ('om', 'iet') ('iet', 'te') ('te', 'blijven') ('blijven', 'betekenen') ('betekenen', 'voor') ('voor', 'men') ('men', 'en') ('en', 'samenlev') ('samenlev', '.') ('.', 'Ik') ('Ik', 'vind') ('vind', 'het') ('het', 'belangrijk') ('belangrijk', 'met') ('met', 'iet') ('iet', 'bezig') ('bezig', 'te') ('te', 'zijn') ('zijn', 'dat') ('dat', 'de') ('de', 'kwaliteit') ('kwaliteit', 'van') ('van', 'leven') ('leven', 'van') ('van', 'mensen') ('mensen', 'op') ('op', 'een') ('een', 'positiev') ('positiev', 'wijz') ('wijz', 'beïnvloedt') ('beïnvloedt', '.') ('.', 'In') ('In', 'mijn') ('mijn', 'eerder') ('eerder', 'werkzaamheden') ('werkzaamheden', 'heb') ('heb', 'ik') ('ik', 'mij') ('mij', 'hiervoor') ('hiervoor', 'ingezet') ('ingezet', 'al') ('al', 'psycholoog') ('psycholoog', 'op') ('op', 'individueel') ('individueel', 'en') ('en', 'groepsniveau') ('groepsniveau', 'en') ('en', 'graag') ('graag', 'zou') ('zou', 'ik') ('ik', 'hele') ('hele', 'samenlevingen') ('samenlevingen', 'willen') ('willen', 'bereiken') ('bereiken', '.') ('.', 'Om') ('Om', '