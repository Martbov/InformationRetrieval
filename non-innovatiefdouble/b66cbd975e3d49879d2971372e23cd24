', 'studenten') ('studenten', '.') ('.', 'Bijvoorbeeld') ('Bijvoorbeeld', 'dat') ('dat', 'ik') ('ik', 'blij') ('blij', 'mag') ('mag', 'zijn') ('zijn', 'met') ('met', 'de') ('de', 'kansen') ('kansen', 'die') ('die', 'ik') ('ik', 'krijg') ('krijg', 'doordat') ('doordat', 'ik') ('ik', 'op') ('op', 'een') ('een', 'universiteit') ('universiteit', 'studeer') ('studeer', '.')