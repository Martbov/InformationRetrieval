') ('sind', 'een') ('een', 'aantal') ('aantal', 'jaar') ('jaar', 'nauw') ('nauw', 'betrokken') ('betrokken', 'bij') ('bij', 'verschillend') ('verschillend', 'schoolschaak') ('schoolschaak', 'projecten') ('projecten', 'waarbij') ('waarbij', 'we') ('we', 'kinderen') ('kinderen', 'vanuit') ('vanuit', 'verschillend') ('verschillend', 'achtergronden') ('achtergronden', 'in') ('in', 'aanrak') ('aanrak', 'te') ('te', 'laten') ('laten', 'komen') ('komen', 'met') ('met', 'schaken') ('schaken', '.')