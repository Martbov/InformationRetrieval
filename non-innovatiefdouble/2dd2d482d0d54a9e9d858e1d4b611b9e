', 'het') ('het', 'Rijk') ('Rijk', 'verder') ('verder', 'kenni') ('kenni', 'te') ('te', 'maken') ('maken', ',') (',', 'verworven') ('verworven', 'kenni') ('kenni', 'uit') ('uit', 'te') ('te', 'breiden') ('breiden', 'en') ('en', 'in') ('in', 'de') ('de', 'praktijk') ('praktijk', 'toe') ('toe', 'te') ('te', 'passen') ('passen', '.') ('.', 'De') ('De', 'ontwikkel') ('ontwikkel', 'van') ('van', 'mijzelf') ('mijzelf', ',') (',', 'zal') ('zal', 'in') ('in', 'de') ('de', 'toekomst') ('toekomst', 'in') ('in', 'vele') ('vele', 'situati') ('situati', 'go') ('go', 'van') ('van', 'pa') ('pa', 'komen') ('komen', ',') (',', 'om') ('om', 'te') ('te', 'laten') ('laten', 'zien') ('zien', 'wie') ('wie', 'ik') ('ik', 'ben') ('ben', ',') (',', 'hoe') ('hoe', 'ik') ('ik', 'werk') ('werk', 'en') ('en', 'wat') ('wat', 'ik') ('ik', 'kan') ('kan', 'toevoegen') ('toevoegen', 'al') ('al', 'persoon') ('persoon', 'en') ('en', 'al') ('al', 'een') ('een', 'Rijkstraine') ('Rijkstraine', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'studi') ('studi', 'ben') ('ben', 'ik') ('ik', 'penningmeest') ('penningmeest', 'geweest') ('geweest', 'voor') ('voor', 'de') ('de', 'Alumni') ('Alumni', 'commissi') ('commissi', 'van') ('van', 'de') ('de', 'Studieverenig') ('Studieverenig', 'Clio') ('Clio', '.') ('.', 'Hierbij') ('Hierbij', 'wa') ('wa', 'het') ('het', 'doel') ('doel', 'om') ('om', 'oud-studiegenoten') ('oud-studiegenoten', 'van') ('van', 'IO/IB') ('IO/IB', 'met') ('met', 'elkaar') ('elkaar', 'in') ('in', 'contact') ('contact', 'te') ('te', 'brengen') ('brengen', ',') (',', 'om') ('om', 'ze') ('ze', 'zo') ('zo', 'nog') ('nog', 'nog') ('nog', 'een') ('een', 'band') ('band', 'te') ('te', 'laten') ('laten', 'hebben') ('hebben', 'met') ('met', 'hun') ('hun', 'studietijd') ('studietijd', '.') ('.', 'Ook') ('Ook', 'wilden') ('wilden', 'we') ('we', 'de') ('de', 'alumni') ('alumni', 'de') ('de', 'mogelijkheid') ('mogelijkheid', 'bieden') ('bieden', 'om') ('om', 'wat') ('wat', 'terug') ('terug', 'te') ('te', 