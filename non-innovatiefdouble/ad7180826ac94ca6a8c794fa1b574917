at', 'je') ('je', 'doet') ('doet', 'en') ('en', 'het') ('het', 'nut') ('nut', 'hiervan') ('hiervan', '.') ('.', 'Met') ('Met', 'bloei') ('bloei', 'doel') ('doel', 'ik') ('ik', 'op') ('op', 'mijn') ('mijn', 'doorzettingsvermogen') ('doorzettingsvermogen', 'en') ('en', 'mijn') ('mijn', 'streven') ('streven', 'naar') ('naar', 'zelfverrijk') ('zelfverrijk', '.') ('.', 'Leren') ('Leren', ',') (',', 'leven') ('leven', ',') (',', 'geloven') ('geloven', 'en') ('en', 'bloeien') ('bloeien', '.') ('.', 'Het') ('Het', 'Rijkstraineeprogramma') ('Rijkstraineeprogramma', 'sluit') ('sluit', 'in') ('in', 'mijn') ('mijn', 'ogen') ('ogen', 'perfect') ('perfect', 'aan') ('aan', 'op') ('op', 'deze') ('deze', 'idealen') ('idealen', '.') ('.', 'Dit') ('Dit', 'betek') ('betek', 'dat') ('dat', 'ik') ('ik', 'me') ('me', 'voor') ('voor', 'de') ('de', 'voll') ('voll', '100') ('100', '%') ('%', 'in') ('in', 'kan') ('kan', 'zetten') ('zetten', 'voor') ('voor', 'dit') ('dit', 'programma') ('programma', '.') ('.', 'Het') ('Het', 'Rijk') ('Rijk', 'zou') ('zou', 'met') ('met', 'mij') ('mij', 'een') ('een', 'leergierig') ('leergierig', ',') (',', 'nieuwsgierig') ('nieuwsgierig', ',') (',', 'optimistisch') ('optimistisch', ',') (',', 'ijverig') ('ijverig', 'en') ('en', 'integer') ('integer', 'werknem') ('werknem', 'rijker') ('rijker', 'zijn') ('zijn', '.') ('.', 'Al') ('Al', 'tiener') ('tiener', 'werd') ('werd', 'ik') ('ik', 'lid') ('lid', 'van') ('van', 'Dwar') ('Dwar', 'en') ('en', 'vegetari') ('vegetari', '.') ('.', 'Nu') ('Nu', 'vind') ('vind', 'ik') ('ik', 'het') ('het', 'interessant') ('interessant', 'om') ('om', 'politiek') ('politiek', 'ontwikkelingen') ('ontwikkelingen', 'van') ('van', 'een') ('een', 'afstandj') ('afstandj', 'te') ('te', 'analyseren') ('analyseren', '.') ('.', 'De') ('De', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'is') ('is', 'wel') ('wel', 'blijven') ('blijven', 'bestaan') ('bestaan', '.') ('.', 'Daarom') ('Daarom', 'ben') ('ben', 'ik') ('ik'