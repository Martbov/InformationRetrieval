eress') ('interess', 'tijden') ('tijden', 'mijn') ('mijn', 'studi') ('studi', 'vooral') ('vooral', 'uitgegaan') ('uitgegaan', 'naar') ('naar', 'de') ('de', 'ontwikkel') ('ontwikkel', 'van') ('van', 'een') ('een', 'kind') ('kind', 'tot') ('tot', 'een') ('een', 'volwassen') ('volwassen', 'autonoom') ('autonoom', 'individu') ('individu', ',') (',', 'welk') ('welk', 'go') ('go', 'presteert') ('presteert', 'in') ('in', 'deze') ('deze', 'maatschappij') ('maatschappij', '.') ('.', 'Mijn') ('Mijn', 'streven') ('streven', 'is') ('is', 'dan') ('dan', 'ook') ('ook', 'om') ('om', 'iedereen') ('iedereen', 'zo') ('zo', 'veel') ('veel', 'mogelijk') ('mogelijk', 'de') ('de', 'kan') ('kan', 'te') ('te', 'geven') ('geven', 'om') ('om', 'zich') ('zich', 'zo') ('zo', 'volledig') ('volledig', 'mogelijk') ('mogelijk', 'te') ('te', 'ontwikkelen') ('ontwikkelen', ',') (',', 'en') ('en', 'zo') ('zo', 'te') ('te', 'kunnen') ('kunnen', 'bijdragen') ('bijdragen', 'aan') ('aan', 'onz') ('onz', 'maatschappij') ('maatschappij', '.') ('.', 'Ook') ('Ook', 'het') ('het', 'onderwerp') ('onderwerp', 'van') ('van', 'mijn') ('mijn', 'onderzoekstag') ('onderzoekstag', 'geeft') ('geeft', 'aan') ('aan', 'dat') ('dat', 'ik') ('ik', 'maatschappelijk') ('maatschappelijk', 'betrokken') ('betrokken', 'ben') ('ben', '.') ('.', 'Hierin') ('Hierin', 'heb') ('heb', 'ik') ('ik', 'namelijk') ('namelijk', 'onderzocht') ('onderzocht', 'hoe') ('hoe', 'mensen') ('mensen', 'kunnen') ('kunnen', 'worden') ('worden', 'beho') ('beho', 'voor') ('voor', 'het') ('het', 'vertonen') ('vertonen', 'van') ('van', 'risicogedrag') ('risicogedrag', 'waarme') ('waarme', 'ze') ('ze', 'zichzelf') ('zichzelf', 'en') ('en', 'anderen') ('anderen', 'in') ('in', 'gevaar') ('gevaar', 'kunnen') ('kunnen', 'brengen') ('brengen', '.') ('.', 'Het') ('Het', 'doel') ('doel', 'van') ('van', 'dit') ('dit', 'onderzoek') ('onderzoek', 'wa') ('wa', 'om') ('om', 'aanwijzingen') ('aanwijzingen', 'te') ('te', 'geven') ('geven', 'hoe') ('hoe', 'men') ('men', '