et') ('het', 'geven') ('geven', 'van') ('van', 'presentati') ('presentati', 'over') ('over', 'maatschappelijk') ('maatschappelijk', 'problemen') ('problemen', 'al') ('al', 'radicalis') ('radicalis', 'en') ('en', 'terrorism') ('terrorism', 'en') ('en', 'reizen') ('reizen', 'naar') ('naar', 'het') ('het', 'verr') ('verr', 'Oosten') ('Oosten', '.') ('.', 'Tijden') ('Tijden', 'mijn') ('mijn', 'Master') ('Master', 'opleid') ('opleid', "'Intern") ("'Intern", 'Conflict') ('Conflict', 'Studi') ('Studi', "'") ("'", 'verdiep') ('verdiep', 'ik') ('ik', 'mij') ('mij', 'verder') ('verder', 'in') ('in', 'de') ('de', 'problematiek') ('problematiek', 'van') ('van', 'het') ('het', 'creëren') ('creëren', 'en') ('en', 'handhaven') ('handhaven', 'van') ('van', 'de') ('de', 'veiligheid') ('veiligheid', 'in') ('in', 'conflictgebieden') ('conflictgebieden', '.') ('.', 'Het') ('Het', 'Rijk') ('Rijk', 'Traineeship') ('Traineeship', 'is') ('is', 'voor') ('voor', 'mij') ('mij', 'de') ('de', 'perfect') ('perfect', 'vervolgstap') ('vervolgstap', 'om') ('om', 'mijn') ('mijn', 'maatschappelijk') ('maatschappelijk', 'vaardigheden') ('vaardigheden', 'niet') ('niet', 'alleen') ('alleen', 'te') ('te', 'gebruiken') ('gebruiken', ',') (',', 'maar') ('maar', 'ook') ('ook', 'verder') ('verder', 'te') ('te', 'ontwikkelen') ('ontwikkelen', '.')