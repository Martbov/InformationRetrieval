eeship') ('traineeship', ',') (',', 'dat') ('dat', 'werken') ('werken', 'en') ('en', 'leren') ('leren', 'combineert') ('combineert', ',') (',', 'sluit') ('sluit', 'perfect') ('perfect', 'aan') ('aan', 'bij') ('bij', 'wat') ('wat', 'ik') ('ik', 'graag') ('graag', 'wil') ('wil', '.') ('.', 'Ik') ('Ik', 'weet') ('weet', 'natuurlijk') ('natuurlijk', 'dat') ('dat', 'ik') ('ik', 'voor') ('voor', 'het') ('het', 'traineeship') ('traineeship', 'het') ('het', 'Twents') ('Twents', 'land') ('land', ',') (',', 'waar') ('waar', 'ik') ('ik', 'nu') ('nu', 'woonachtig') ('woonachtig', 'ben') ('ben', ',') (',', 'zal') ('zal', 'moeten') ('moeten', 'verlaten') ('verlaten', '.') ('.', 'Dat') ('Dat', 'heb') ('heb', 'ik') ('ik', 'er') ('er', 'graag') ('graag', 'voor') ('voor', 'over') ('over', 'en') ('en', 'misschien') ('misschien', 'is') ('is', 'wat') ('wat', 'meer') ('meer', 'Twents') ('Twents', 'inbreng') ('inbreng', 'in') ('in', 'het') ('het', 'Westen') ('Westen', 'helema') ('helema', 'zo') ('zo', 'gek') ('gek', 'nog') ('nog', 'niet') ('niet', '.') ('.', 'Sind') ('Sind', 'een') ('een', 'aantal') ('aantal', 'jaren') ('jaren', 'ben') ('ben', 'ik') ('ik', 'lid') ('lid', 'van') ('van', 'de') ('de', 'jongerenraad') ('jongerenraad', 'van') ('van', 'de') ('de', 'plaatselijk') ('plaatselijk', 'Rabobank') ('Rabobank', '.') ('.', 'Met') ('Met', 'de') ('de', 'jongerenraad') ('jongerenraad', ',') (',', 'waarbij') ('waarbij', 'ik') ('ik', 'van') ('van', 'meet') ('meet', 'af') ('af', 'aan') ('aan', 'betrokken') ('betrokken', 'ben') ('ben', 'geweest') ('geweest', ',') (',', 'trachten') ('trachten', 'wij') ('wij', 'de') ('de', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', ',') (',', 'welk') ('welk', 'de') ('de', 'Rabobank') ('Rabobank', 'hoog') ('hoog', 'in') ('in', 'het') ('het', 'vaandel') ('vaandel', 'heeft') ('heeft', 'staan') ('staan', ',') (',', 'over') ('over', 'te') ('te', 'brengen') ('brengen', 'op') ('op', 'de') ('de', 'lokal') ('lokal', 'gemeenschap') ('gemeensc