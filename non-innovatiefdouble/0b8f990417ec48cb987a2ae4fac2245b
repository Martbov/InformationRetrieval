') ('internationa', 'georiënteerd') ('georiënteerd', ':') (':', 'ik') ('ik', 'heb') ('heb', 'in') ('in', 'Spanj') ('Spanj', 'en') ('en', 'Noorwegen') ('Noorwegen', 'gestudeerd') ('gestudeerd', 'en') ('en', 'in') ('in', 'Californië') ('Californië', 'gewerkt') ('gewerkt', '.') ('.', 'Ik') ('Ik', 'spreek') ('spreek', 'zeer') ('zeer', 'go') ('go', 'Engel') ('Engel', ',') (',', 'go') ('go', 'Spaan') ('Spaan', 'en') ('en', 'redelijk') ('redelijk', 'Fran') ('Fran', ',') (',', 'Duit') ('Duit', 'en') ('en', 'Noor') ('Noor', '.') ('.', 'Ik') ('Ik', 'doe') ('doe', 'graag') ('graag', 'moeit') ('moeit', 'om') ('om', 'te') ('te', 'communiceren') ('communiceren', 'met') ('met', 'de') ('de', 'mensen') ('mensen', 'om') ('om', 'mij') ('mij', 'heen') ('heen', '.') ('.', 'Ik') ('Ik', 'ben') ('ben', 'ambitieu') ('ambitieu', ',') (',', 'zoal') ('zoal', 'blijkt') ('blijkt', 'uit') ('uit', 'mijn') ('mijn', 'sportiev') ('sportiev', 'prestati') ('prestati', 'op') ('op', 'roeigebi') ('roeigebi', '.') ('.', 'Ik') ('Ik', 'streef') ('streef', 'zelf') ('zelf', 'naar') ('naar', 'een') ('een', 'harmonisch') ('harmonisch', 'leefomgev') ('leefomgev', '.') ('.', 'Dat') ('Dat', 'wil') ('wil', 'zeggen') ('zeggen', 'dat') ('dat', 'ik') ('ik', 'milieubewust') ('milieubewust', 'probeer') ('probeer', 'te') ('te', 'leven') ('leven', 'en') ('en', 'dat') ('dat', 'ik') ('ik', 'anderen') ('anderen', 'met') ('met', 'een') ('een', 'open') ('open', 'vizier') ('vizier', 'tegemoet') ('tegemoet', 'treed') ('treed', '.') ('.', 'Ik') ('Ik', 'werk') ('werk', 'momenteel') ('momenteel', 'op') ('op', 'een') ('een', 'middelbar') ('middelbar', 'school') ('school', '.') ('.', 'Voor') ('Voor', 'mij') ('mij', 'is') ('is', 'het') ('het', 'werken') ('werken', 'met') ('met', 'jongeren') ('jongeren', 'een') ('een', 'mogelijkheid') ('mogelijkheid', 'om') ('om', 'samen') ('samen', 'te') ('te', 'werken') ('werken', 'aan') ('aan', 'een') ('een', 'open') ('open', 'en') ('en', 'respectvol') ('respectvol', 'samenlev') ('samenlev', '.') ('.