', 'al') ('al', 'groepsleerkracht') ('groepsleerkracht', 'word') ('word', 'ik') ('ik', 'vaak') ('vaak', 'met') ('met', 'allerlei') ('allerlei', 'maatschappelijk') ('maatschappelijk', 'problemen') ('problemen', 'geconfronteerd') ('geconfronteerd', '.') ('.', 'Waardoor') ('Waardoor', 'mijn') ('mijn', 'belangstel') ('belangstel', 'voor') ('voor', 'de') ('de', 'verschillend') ('verschillend', 'oplossingen') ('oplossingen', 'voor') ('voor', 'deze') ('deze', 'vraagstukken') ('vraagstukken', 'is') ('is', 'toegenomen') ('toegenomen', '.') ('.', 'Mijn') ('Mijn', 'vriendenkr') ('vriendenkr', 'bestaat') ('bestaat', 'uit') ('uit', 'mensen') ('mensen', 'met') ('met', 'verschillend') ('verschillend', 'culturel') ('culturel', 'en') ('en', 'social') ('social', 'achtergronden') ('achtergronden', '.') ('.', 'Dat') ('Dat', 'is') ('is', 'een') ('een', 'van') ('van', 'de') ('de', 'reden') ('reden', 'voor') ('voor', 'mijn') ('mijn', 'belangstel') ('belangstel', 'voor') ('voor', 'burgerschap') ('burgerschap', 'en') ('en', 'social') ('social', 'integrati') ('integrati', '.')