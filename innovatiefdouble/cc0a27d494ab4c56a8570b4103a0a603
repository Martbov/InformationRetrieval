is') ('is', 'dat') ('dat', 'ik') ('ik', 'een') ('een', 'zo') ('zo', 'breed') ('breed', 'mogelijk') ('mogelijk', 'netwerk') ('netwerk', 'van') ('van', 'vrienden') ('vrienden', 'heb') ('heb', ',') (',', 'op') ('op', 'dit') ('dit', 'mijn') ('mijn', 'manier') ('manier', 'zorg') ('zorg', 'ik') ('ik', 'ervoor') ('ervoor', 'om') ('om', 'met') ('met', 'beid') ('beid', 'benen') ('benen', 'op') ('op', 'de') ('de', 'grond') ('grond', 'te') ('te', 'blijven') ('blijven', 'staan') ('staan', '.')