') ('mijn', 'Masterspecialisati') ('Masterspecialisati', 'Social') ('Social', 'en') ('en', 'Organisati') ('Organisati', 'Psychologi') ('Psychologi', 'ben') ('ben', 'ik') ('ik', 'getraind') ('getraind', 'om') ('om', 'social') ('social', 'problemen') ('problemen', 'te') ('te', 'analyseren') ('analyseren', 'en') ('en', 'op') ('op', 'te') ('te', 'lossen') ('lossen', '.') ('.', 'Teven') ('Teven', 'voel') ('voel', 'ik') ('ik', 'mij') ('mij', 'zeer') ('zeer', 'aangetrokken') ('aangetrokken', 'tot') ('tot', 'maatschappelijk') ('maatschappelijk', 'onderwerpen') ('onderwerpen', '.') ('.', 'Dit') ('Dit', 'blijkt') ('blijkt', 'uit') ('uit', 'het') ('het', 'Masterthesis/onderzoek') ('Masterthesis/onderzoek', 'dat') ('dat', 'ik') ('ik', 'heb') ('heb', 'gedaan') ('gedaan', 'naar') ('naar', 'de') ('de', 'rol') ('rol', 'van') ('van', 'eercultuur') ('eercultuur', 'in') ('in', 'relati') ('relati', 'en') ('en', 'de') ('de', 'invlo') ('invlo', 'hiervan') ('hiervan', 'op') ('op', 'conflicten') ('conflicten', ',') (',', 'emoti') ('emoti', 'en') ('en', 'conflicthanteringstrategieen') ('conflicthanteringstrategieen', '.') ('.', 'Dit') ('Dit', 'blijkt') ('blijkt', 'ook') ('ook', 'uit') ('uit', 'het') ('het', 'Internship/onderzoek') ('Internship/onderzoek', 'naar') ('naar', 'effectiviteit') ('effectiviteit', 'aanpak') ('aanpak', 'jongerengroepen') ('jongerengroepen', 'dat') ('dat', 'ik') ('ik', 'heb') ('heb', 'gedaan') ('gedaan', 'bij') ('bij', 'Politi') ('Politi', 'Holland') ('Holland', 'Midden') ('Midden', '.')