en') ('en', 'uiteraard') ('uiteraard', 'constant') ('constant', 'in') ('in', 'ontwikkel') ('ontwikkel', '.') ('.', 'Een') ('Een', 'ideal') ('ideal', 'werkomgev') ('werkomgev', 'voor') ('voor', 'mijn') ('mijn', 'enthousiast') ('enthousiast', 'en') ('en', 'oplossingsgericht') ('oplossingsgericht', 'mentaliteit') ('mentaliteit', '.') ('.', 'Het') ('Het', 'woord') ('woord', 'ontwikkel') ('ontwikkel', 'is') ('is', 'op') ('op', 'vele') ('vele', 'vlakken') ('vlakken', 'belangrijk') ('belangrijk', 'voor') ('voor', 'mij') ('mij', 'en') ('en', 'het') ('het', 'Rijkstraineeprogramma') ('Rijkstraineeprogramma', 'sluit') ('sluit', 'hier') ('hier', 'naadloo') ('naadloo', 'op') ('op', 'aan') ('aan', '.') ('.', 'Maatschappelijk') ('Maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'kan') ('kan', 'zich') ('zich', 'op') ('op', 'vele') ('vele', 'manieren') ('manieren', 'uiten') ('uiten', ',') (',', 'voor') ('voor', 'de') ('de', 'een') ('een', 'is') ('is', 'dit') ('dit', 'een') ('een', 'stem') ('stem', 'uitbrengen') ('uitbrengen', 'tijden') ('tijden', 'verkiezingstijd') ('verkiezingstijd', 'terwijl') ('terwijl', 'een') ('een', 'ander') ('ander', 'tijden') ('tijden', 'verjaardagen') ('verjaardagen', 'de') ('de', 'discussi') ('discussi', 'aangaat') ('aangaat', 'over') ('over', 'het') ('het', 'onderwij') ('onderwij', '.') ('.', 'Voor') ('Voor', 'mij') ('mij', 'is') ('is', 'maatschappelijk') ('maatschappelijk', 'betrokkenheid') ('betrokkenheid', 'begonnen') ('begonnen', 'op') ('op', 'de') ('de', 'middelbar') ('middelbar', 'school') ('school', 'waar') ('waar', 'ik') ('ik', 'ben') ('ben', 'begonnen') ('begonnen', 'met') ('met', 'het') ('het', 'volgen') ('volgen', 'van') ('van', 'het') ('het', 'journaal') ('journaal', '.') ('.', 'Hierna') ('Hierna', 'bracht') ('bracht', 'het') ('het', 'internet') ('internet', 'me') ('me', 'de') ('de', 'mogelijkheid') ('mogelijkheid', 'om') ('om', 'me') ('me', 'in') ('in', 'te') ('te', 'kunnen') ('kunnen', 'lezen') ('lezen', 'in') ('in', 'de') ('de', 'onderw