en') ('ben', 'ik') ('ik', 'al') ('al', 'stagiair') ('stagiair', 'werkzaam') ('werkzaam', 'geweest') ('geweest', 'bij') ('bij', 'de') ('de', 'Adviesraad') ('Adviesraad', 'International') ('International', 'Vraagstukken') ('Vraagstukken', 'binnen') ('binnen', 'het') ('het', 'Ministeri') ('Ministeri', 'van') ('van', 'Buitenlands') ('Buitenlands', 'Zaken') ('Zaken', '.') ('.', 'In') ('In', 'mijn') ('mijn', 'vrije') ('vrije', 'tijd') ('tijd', 'ben') ('ben', 'ik') ('ik', 'bij') ('bij', 'de') ('de', 'plaatselijk') ('plaatselijk', 'basketbalclub') ('basketbalclub', 'actief') ('actief', 'al') ('al', '(') ('(', 'vervangend') ('vervangend', ')') (')', 'trainer/coach') ('trainer/coach', 'en') ('en', 'scheidsrecht') ('scheidsrecht', '.') ('Het', 'rijk', 'spreekt') ('rijk', 'spreekt', 'mij') ('spreekt', 'mij', 'al') ('mij', 'al', 'organisati') ('al', 'organisati', 'enorm') ('organisati', 'enorm', 'aan') ('enorm', 'aan', '.') ('aan', '.', 'Het') ('.', 'Het', 'uitwerken') ('Het', 'uitwerken', 'en') ('uitwerken', 'en', 'evalueren') ('en', 'evalueren', 'van') ('evalueren', 'van', 'beleid') ('van', 'beleid', 'en') ('beleid', 'en', 'het') ('en', 'het', 'daarvoor') ('het', 'daarvoor', '(') ('daarvoor', '(', 'onder') ('(', 'onder', 'ander') ('onder', 'ander', ')') ('ander', ')', 'benodigd') (')', 'benodigd', 'analytisch') ('benodigd', 'analytisch', 'vermogen') ('analytisch', 'vermogen', ',') ('vermogen', ',', 'sluit') (',', 'sluit', 'aan') ('sluit', 'aan', 'bij') ('aan', 'bij', 'mijn') ('bij', 'mijn', 'interess') ('mijn', 'interess', 'en') ('interess', 'en', 'persoonlijk') ('en', 'persoonlijk', 'kracht') ('persoonlijk', 'kracht', '.') ('kracht', '.', 'Daadkracht') ('.', 'Daadkracht', ',') ('Daadkracht', ',', 'strategisch') (',', 'strategisch', 'denken') ('strategisch', 'denken', 'en') ('denken', 'en', 'analytisch') ('en', 'analytisch', 'vermogen') ('analytisch', 'vermogen', 'zijn') ('vermogen', 'zijn', 'termen') ('zijn', 'termen', 'die') ('termen', 'die', 'bij') ('die', 'bij', 'mij') ('b